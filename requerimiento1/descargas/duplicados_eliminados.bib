  title = {Realization and Experimental Research on Immersive Visual Communication System Based on Real-Time Rendering},
  url = {https://doi.org/10.1145/3728725.3728814},
  year = {2025}
}

@inproceedings{10.1145/3728725.3728818,
  abstract = {With the rapid development of social media, internet public opinion has a profound impact on social stability and economic development. At present, common monitoring models generally have problems such as insufficient accuracy or weak generalization ability. Therefore, This paper proposes an internet public opinion monitoring model based on stacked ensemble learning, which aims to realize the real-time monitoring. The model selects combines random forest (RF), logistic regression (LR), support vector machine (SVM) group as the base learner. And integrates the prediction results of the base-learners through the meta learners (GBM) to monitor public opinion in real time. The experimental results show that the empirical model achieves 0.940 accuracy and an F1 score of 0.896, which significantly outperforms other ensemble methods. It can effectively monitor the scale and trend of public opinion of internet comments, and provide strong support for decision makers.},
  address = {New York, NY, USA},
  author = {Zhu, Qifei and Tang, Jiangang and Cao, Haoshun},
  booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
  doi = {10.1145/3728725.3728818},
  isbn = {9798400713453},
  keywords = {Empirical Research, Internet Public Opinion Monitoring, Stacked Ensemble Learning},
  location = {},
  numpages = {7},
  pages = {591–597},
  publisher = {Association for Computing Machinery},
  series = {GAIIS '25},
  title = {An Internet Public Opinion Monitoring Model Based on Stacked Ensemble Learning},
  url = {https://doi.org/10.1145/3728725.3728818},
  year = {2025}
}

@inbook{10.1145/3728725.3728820,
  abstract = {In the intricate environment where crops and weeds grow intermingled, an effective solution to this problem is to precisely spray herbicides on weeds while accurately avoiding crops. The technical reliance in this process is the focus of this paper, which involves classifying, identifying, and locating weeds and crops. Currently, deep learning is widely used for image recognition and classification. However, the complex characteristics of the intermingled growth of weeds and crops are the main factors affecting the extraction of weed features. Based on an extensive review of domestic and foreign literature, and combining the research progress and methods of deep learning in the field of image recognition, this paper designs a method that can identify weeds with complex characteristics, effectively improving the accuracy of weed image recognition. This provides a favorable reference for the rapid and precise targeted spraying of herbicides in agricultural intelligent equipment or other devices beneficial to human life.},
  address = {New York, NY, USA},
  author = {Chen, Xunlin and Li, Xiaoju and Chen, Ziyong},
  booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
  isbn = {9798400713453},
  numpages = {5},
  pages = {605–609},
  publisher = {Association for Computing Machinery},
  title = {Research on Weed Identification Based on Deep Learning},
  url = {https://doi.org/10.1145/3728725.3728820},
  year = {2025}
}

@inbook{10.1145/3728725.3728821,
  abstract = {Aiming at the traditional fall detection algorithms that suffer from low detection accuracy and poor real-time performance in dealing with small targets and multi-scale features in complex scenes, this study proposes a fall detection algorithm based on an improved YOLOv8 model. First, the SCConv module is utilized to combine with the C2f module in the YOLOv8 backbone network to construct the C2fSC module, which enhances the feature extraction capability of the model while reducing the computational complexity. Second, the SPPF module is integrated with the BAM attention mechanism to weaken the influence of background interference. Finally, WIoU is used to replace the original loss function to optimize the positioning accuracy of the bounding box and enhance the overall performance of the model. The experimental results show that compared with the original YOLOv8 model, the improved model improves 4.9\%, 4.2\%, and 4.4\% in P, R, and mAP metrics, respectively, and the number of parameters and FLOPs are only 2.8M and 7.7G, which proves that the improved model has excellent robustness and effectiveness.},
  address = {New York, NY, USA},
  author = {Cai, Litao and Liu, Mingmei and Feng, Youbing and Xiong, Feng},
  booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
  isbn = {9798400713453},
  numpages = {6},
  pages = {610–615},
  publisher = {Association for Computing Machinery},
  title = {Research on fall detection algorithm based on improved YOLOv8},
  url = {https://doi.org/10.1145/3728725.3728821},
  year = {2025}
}

@inbook{10.1145/3728725.3728828,
  abstract = {The rapid development of computer technology has brought computer graphics a qualitative leap in graphic images and visual communication. Computer graphics transforms digital information into image information while conducting digital processing and visualization of graphic images. Combining with the development process of computer graphics, this paper considers the application field of computer graphics, discusses the connotation deduction of multi-modal translation resource. Through case analysis of the Visionbank, this paper validates and advances computer graphics technology to optimize multi-modal translation resource, and realizes the comprehensive utilization of multi-modal translation resource.},
  address = {New York, NY, USA},
  author = {Xu, Xintao},
