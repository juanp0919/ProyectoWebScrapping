@inproceedings{10.1145/3766918.3766927,
  abstract = {This study proposes a direct, dynamic approach to evaluating the resilience of industrial chains, using complex network disruption simulations. The Chinese photovoltaic industry serves as a case study. First, we mapped the industrial chain and gathered data on production enterprises. Next, we constructed the overall industrial network and its upstream, midstream, and downstream sub-networks, followed by a preliminary analysis of the network's features. Finally, we simulated network disruptions to assess and analyze the resilience of the industrial chain. The results are as follows: (1) In the Chinese photovoltaic industry, the network density and average degree of each sub-network are generally high, indicating that enterprises are tightly connected, with strong agglomeration and transmission properties. (2) Overall, the Chinese photovoltaic industrial chain demonstrates strong resilience, with the downstream sub-network showing the highest resilience, followed by the midstream, and the upstream exhibiting the lowest resilience.},
  address = {New York, NY, USA},
  author = {Yang, Tongyu},
  booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence for Business},
  doi = {10.1145/3766918.3766927},
  isbn = {9798400716027},
  keywords = {Complex Network, Disruption Simulation, Industrial Chain Resilience, Photovoltaic Field},
  location = {},
  numpages = {8},
  pages = {54–61},
  publisher = {Association for Computing Machinery},
  series = {GAIB '25},
  title = {Assessment of the Industrial Chain Resilience under Complex Network Disruption Simulation: A Case Study of the Chinese Photovoltaic Field},
  url = {https://doi.org/10.1145/3766918.3766927},
  year = {2025}
}

@inproceedings{10.1145/3766918.3766949,
  abstract = {Small and Medium Enterprises (SMEs) represent a critical component of global economic infrastructure, yet traditional credit risk assessment methodologies often fail to capture their complex financial dynamics. This research presents an innovative intelligent framework for SME credit risk assessment through multi-dimensional data fusion techniques. The proposed methodology integrates traditional financial indicators with alternative data sources including transactional behavior patterns, supply chain relationships, and macroeconomic variables. The framework employs advanced machine learning algorithms including ensemble methods and deep neural networks to process heterogeneous data streams. Experimental validation demonstrates superior performance compared to conventional approaches, achieving 92.3\% accuracy in risk classification with reduced false positive rates by 34.7\%. The integration of multi-dimensional features enhances predictive capability while maintaining computational efficiency. This research contributes to the advancement of intelligent financial risk assessment systems and provides practical implications for financial institutions seeking enhanced SME evaluation capabilities.},
