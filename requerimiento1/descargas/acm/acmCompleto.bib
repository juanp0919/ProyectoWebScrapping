}

@inproceedings{10.1145/3665348.3665365,
author = {Wang, Yingqiang and Serrano, Elcid},
title = {Research on personalized music recommendation model based on human physiological signals},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665365},
doi = {10.1145/3665348.3665365},
abstract = {Music recommendation systems have become one of the research directions that have attracted much attention in the field of digital entertainment. However, current music recommendation methods are mainly recommended through collaborative filtering, content-based, and manual rule methods, and cannot be recommended based on the user's real-time status, resulting in the recommended music does not meet the real-time needs of users. In response to this situation, this paper proposes a personalized music recommendation model based on human physiological signals. This model first analyzes the user's physiological data, such as heart rate, electrodermal activity, etc., and uses a graph convolutional neural network algorithm to perceive the user's real-time emotional state. It then achieves more accuracy and personalization music recommendations based on the relationship model between emotion and music selection. This research not only expands the research scope in the field of music recommendation, but also provides new ideas and methods for the further development of intelligent music recommendation systems.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {89–93},
numpages = {5},
