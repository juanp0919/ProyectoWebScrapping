}

@inproceedings{10.1145/3726010.3726021,
author = {Du, Xiaoru and Chen, Yihan and Le, Ziyi and Wu, Jingwen},
title = {A Study on the Influence Path of AIGC Technology on Creativity Generation of Home Textile Designers},
year = {2025},
isbn = {9798400712845},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3726010.3726021},
doi = {10.1145/3726010.3726021},
abstract = {Previous studies have focused on the role of AIGC technology in improving design efficiency and quality, but its specific impact on designers' creativity generation paths has not been explored in depth. This study aims to explore the specific impact of AIGC technology on the creativity path of home textile designers through the SOR theoretical model. Through qualitative and quantitative research methods, this paper analyzes the application of AIGC technology in home textile design, revealing its role in improving design efficiency, optimizing design quality, and promoting human-machine collaboration. The study shows that AIGC technology not only redefines the mode of idea generation, but also provides new impetus for process design innovation. Based on the research results, this paper proposes a strategy to optimize the design process of home textiles, which provides theoretical support and practical guidance for the further application and design practice of AIGC technology, as well as new ideas for future research and industrial applications.},
booktitle = {Proceedings of the 2024 International Conference on Artificial Intelligence, Digital Media Technology and Interaction Design},
