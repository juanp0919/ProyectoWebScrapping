doi = {10.1145/3766918.3766969},
abstract = {The way colleges and unis manage their policy documents has always had problems with stuff being all over the place and it being hard to find what you're looking for. This analysis puts forward a method for creating a knowledge graph for university policy documents using natural language processing, as well as a technical framework for identifying policy entities and extracting relationships between them. It also develops a system for combining knowledge and making inferences, and achieves organised representation and smart use of policy documents. The results demonstrate the effectiveness of the method in enhancing policy retrieval accuracy, facilitating policy consistency testing and optimising policy interpretation, thereby providing a technical foundation for the modernisation of university policy governance. This method can also be applied to other types of policy documents in knowledge management.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence for Business},
pages = {305–310},
numpages = {6},
keywords = {entity recognition, knowledge graph, natural language processing, policy documents, relationship extraction, university governance},
