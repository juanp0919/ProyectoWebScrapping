doi = {10.1145/3766918.3766980},
abstract = {Amid intensifying algorithmic mediation, collectible toy brands confront a loyalty crisis as generative AI redirects consumer attention from iconic IPs to context-matched novelties. The field lacks a unifying framework explaining how AI reallocates decision agency. We address this gap by developing and testing the Decision Agency Model. Using a between-subjects experiment, we exposed 100 verified Pop Mart consumers to either AI-curated or self-selected product sets across 20 usage scenarios and 100 SKUs. AI recommendations reduced IP selection by 46 percentage points, elevated scenario-congruent items to top rank, and cut composite satisfaction by 0.5 points despite faster choices. The findings reveal that scenario tags supplant brand salience as the dominant cue and uncover an efficiency–affect trade-off inherent in algorithmic curation. We contribute a parsimonious model of AI-driven loyalty erosion and provide actionable guidance for hybrid recommender architectures.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence for Business},
pages = {375–382},
numpages = {8},
keywords = {Brand loyalty, Consumer behavior, Decision agency, Generative AI, Scenario tags, Trendy toys},
location = {
},
series = {GAIB '25}
}

