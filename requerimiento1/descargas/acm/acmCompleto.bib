@inproceedings{10.1145/3746275.3762215,
author = {Skoularikis, Anastasios and Papadopoulos, Stefanos-Iordanis and Papadopoulos, Symeon and Petrantonakis, Panagiotis C.},
title = { 'Humor, Art, or Misinformation?': A Multimodal Dataset for Intent-Aware Synthetic Image Detection},
year = {2025},
isbn = {9798400720574},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3746275.3762215},
doi = {10.1145/3746275.3762215},
abstract = {Recent advances in multimodal AI have enabled progress in detecting synthetic and out-of-context content. However, existing efforts largely overlook the intent behind AI-generated images. To fill this gap, we introduce S-HArM, a multimodal dataset for intent-aware classification, comprising 9,576 ''in the wild'' image–text pairs from Twitter/X and Reddit, labeled as Humor/Satire, Art, or Misinformation. Additionally, we explore three prompting strategies (image-guided, description-guided, and multimodally-guided) to construct a large-scale synthetic training dataset with Stable Diffusion. We conduct an extensive comparative study including modality fusion, contrastive learning, reconstruction networks, attention mechanisms, and large vision-language models. Our results show that models trained on image- and multimodally-guided data generalize better to ''in the wild'' content, due to preserved visual context. However, overall performance remains limited, highlighting the complexity of inferring intent and the need for specialized architectures.},
booktitle = {Proceedings of the 2nd International Workshop on Diffusion of Harmful Content on Online Web},
pages = {95–104},
numpages = {10},
keywords = {multimodal deep learning, synthetic image detection, ai-generated content, intent-aware classification},
location = {Ireland},
series = {DHOW '25}
}

@inproceedings{10.1145/3711670.3764647,
author = {Akorli-Coffie, Ernestina Akorfa and Bikmohammadi, Mina and Shrestha, Kalpana and Clark, Erin and Eble, Michelle F.},
title = {Technical Communication's Role in Understanding GenAI Technologies Across Cultural Boundaries},
year = {2025},
isbn = {9798400714443},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3711670.3764647},
doi = {10.1145/3711670.3764647},
abstract = {This presentation examines the role of technical communicators in shaping ethical and equitable implementations of generative AI (genAI) technologies with a focus on intercultural communication and localized knowledge-making. Through three use cases, the authors demonstrate how technical communicators can address algorithmic biases and promote inclusive practices by exploring how women activists in South Asia develop algorithmic literacies for digital advocacy despite gender and geographical digital divides; analyzing university chatbots' limitations in serving international students, revealing issues with machine learning bias, accessibility, and linguistic barriers; and theorizing an AI-powered chatbot for a Zine Lab focused on gender justice. The authors argue that understanding specific local contexts and cultures is essential when implementing genAI technologies, especially for marginalized communities, and that technical communicators can play a crucial role in developing more equitable technological futures.},
booktitle = {Proceedings of the 43rd ACM International Conference on Design of Communication},
pages = {224–226},
numpages = {3},
keywords = {Global South, digital literacies, ethics, genAI technologies, intercultural/international technical communication, social justice, technical communication, use cases},
location = {
},
series = {SIGDOC '25}
}

@inproceedings{10.1145/3715070.3748275,
author = {Freeman, Guo and Mynatt, Elizabeth D. and Lampe, Cliff and Candello, Heloisa and Inkpen, Kori and Goyal, Nitesh},
title = {The Impact of Generative AI on the CSCW Landscape: Insights from HCI Education, Industry Dynamics, and Funding Perspectives},
year = {2025},
