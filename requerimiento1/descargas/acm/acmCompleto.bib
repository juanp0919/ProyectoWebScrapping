series = {GAIIS '24}
}

@inproceedings{10.1145/3665348.3665351,
author = {Wang, Xuekai and Yang, Zhuocheng and Zhang, Hengbo and Li, Yiming and Wang, Xinke and Cui, Wei},
title = {Research on Traffic Congestion Active Control Technology for Expressway},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665351},
doi = {10.1145/3665348.3665351},
abstract = {This paper takes the active traffic control strategy under the expressway congestion scenario as the research object, investigates the research and application status of active traffic control system at home and abroad, and focuses on the analysis of the two most effective control strategies of active traffic control technology, and introduces the basic principle and control algorithm of the variable speed limit control strategy and the on-ramp control strategy in detail. On this basis, the collaborative control strategy combining variable speed limit control and on-ramp is proposed, and the applicability of the collaborative control strategy under different traffic conditions is analyzed. According to the demand of the main line of the expressway, the further deterioration of traffic congestion is prevented from the source, and the traffic flow on the main line is dynamically optimized and adjusted. Effectively improve highway capacity and safe driving level. Finally, it also introduces the basic elements of active traffic control system design, which lays a foundation for the later project implementation and landing. Based on this strategy, traffic managers can be provided with refined traffic control services, which not only meet the needs of travel users but also meet the operational management needs of owners, and effectively improve the traffic efficiency and safety level of expressways.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {13–17},
numpages = {5},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3665348.3665393,
author = {Tang, Jiale},
title = {Dual-Decoder Importance-Aware Gated Network for RGB-T Salient Objection Detection},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665393},
doi = {10.1145/3665348.3665393},
abstract = {Most recent algorithms of RGB-T salient object detection have two issues remain unresolved. The first one is that existing methods don't fully explore and exploit the potentials of complementary of different modalities and multi-type cues of images, which play a vital role in achieving accurate results. And the second is that it is difficult to determine which of the two modalities contributes more in the feature fusion stage.To address these problems, we propose a dual-decoder importance-aware gated network called LNet. Specifically, we set the dual-decoder architecture to solve the cross-modality interaction problem. It can dig out the characteristics of two modalities, and fuse the cross-modality complementarity information more effectively. In particular, the importance-aware gated part Screening Mechanism (SM) can suppress saliency bias and let model avoid obtaining the results dominated by one modality.Experimental results demonstrate that the proposed LNet achieves better performance than the 11 state-of-the-art on three public datasets.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {261–265},
numpages = {5},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3665348.3665380,
author = {Yan, Liuqing and Yu, Shihu},
title = {Study on English expression polishing system based on deep learning},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665380},
doi = {10.1145/3665348.3665380},
abstract = {In general, grammar polishing systems recombine words and their order of combination based on the comparison results of word combinations and large vocabulary grammar data to achieve error polishing. However, this process is limited by the storage capacity of the large vocabulary grammar, which hampers the effectiveness and efficiency of error polishing. To overcome this constraint, deep learning algorithms are employed. These algorithms involve reconstructing the underlying hardware framework and optimizing the error polishing calculation parameters of the system software using deep learning algorithms. This allows for global replacement of deep learning algorithms, enhances the system's self-learning ability for error polishing, and frees it from the limitations of large vocabulary grammar information. The test results demonstrate that the continuous error polishing ability of this approach is superior. Among the error polishing systems tested, the designed system exhibits the best overall stress test performance, showcasing strong stability and control. The overall stability demonstrated by the stress test aligns with the standards and requirements of practical applications. As a result, the design system possesses good overall error polishing ability, system stability, and practical applicability.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {179–183},
numpages = {5},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3665348.3665350,
author = {Ma, Yue and Jia, Yunhuan and Zhao, Jichang},
title = {A model of risk contagion for mutual funds considering fund redemptions and asset fire sales},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665350},
doi = {10.1145/3665348.3665350},
abstract = {This study proposes a theoretical model for analyzing the risk contagion process resulting from unexpected decreases in fund NAV, which can trigger fire sales and asset price impacts. The model demonstrates that the effect of redemptions on asset prices is linear, and the subsequent impact on fund performance is contingent on the initial shock level and asset liquidity diversity. Our findings suggest that funds those holding illiquid stocks face higher liquidity risks, and the liquidity level of underlying common assets between mutual fund portfolios dictates the extent of losses in the liquidated stock prices and the connected fund NAVs. Second, higher liquidity levels of common assets result in lower level of portfolio overlap. These findings highlight the urgency of establishing unified liquidity standard measurements and a cash buffer criterion. To manage fund stability and liquidity effectively, it is essential to implement measures such as minimum cash requirements and optimized asset allocation. These strategies have been proven to offer valuable insights for achieving financial success.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {8–12},
numpages = {5},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3665348.3665368,
author = {Jin, Bo},
title = {Neural Machine Translation based on Semantic Word Replacement},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665368},
doi = {10.1145/3665348.3665368},
abstract = {In recent years, there has been significant progress in the quality of neural machine translation technology, largely attributed to the abundance of high-quality bilingual parallel sentences. However, low-resource languages face a challenge as the availability of bilingual parallel corpora is limited, leading to subpar translation performance. To tackle this issue, this paper introduces a data augmentation approach based on semantic word replacement specifically designed for neural machine translation of low-resource languages. The method comprises two key steps: firstly, expanding the corpus of low-resource languages through semantic word replacement techniques; secondly, utilizing a neural grammar correction model to proofread the augmented bilingual parallel corpora. Subsequently, these corrected corpora are merged with the original corpus to mitigate the data scarcity for low-resource languages. Experimental evaluations were conducted on various low-resource language pairs (including Mongolian-Chinese, Tibetan-Chinese, Uyghur-Chinese, Arabic-Chinese) as well as English-German and German-English translation tasks. The results demonstrate that our proposed method significantly enhances the translation quality of low-resource languages, achieving a maximum BLEU score improvement of up to 3.06 compared to the baseline system, underscoring the efficacy of the approach.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {106–112},
numpages = {7},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3665348.3665378,
author = {Xiong, Shutao and Qu, Jixing and Xue, Wang and Li, Yanjun},
title = {Deep learning based defect classification method for pressure gas cylinders},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665378},
doi = {10.1145/3665348.3665378},
abstract = {Infrared image-based defect detection of gas cylinders is a key link in the manufacturing and inspection of gas cylinders, accurately distinguish the type, distribution, number and area of defects in the image is the basis for determining the quality of gas cylinders, based on the deep learning of intelligent defect classification methods is the mainstream direction of current research. This paper proposes a comprehensive technology of defect classification method for pressure gas cylinders based on on-site collection of gas cylinder defect data set. The main work is as follows: 1) Aiming at the problem of insufficient number and unbalanced distribution of field-collected defect samples, a DA-DCGAN based on the dual-channel attention mechanism is designed to increase the sample data, and a self-attention generator and a dual-channel discriminator are constructed to ensure the accuracy and reliability of the generated samples; 2) Based on the augmented and broadened dataset, a design of a cascaded HRNet classification network is proposed to increase the quality of pressure gas cylinders by means of global feature extraction, and local feature extraction for feature fusion to achieve multiple classification of defects based on infrared gas cylinder images. Compared with the mainstream classification methods DenseNet, ResNet, and VGG, the cascaded HRNet method improves the average AUC by 6.1\% and achieves the highest scores in two defect types. For temperature fluctuation experiments under real working conditions, good performance is achieved, proving the effectiveness and anti-interference of the improved model.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {167–172},
numpages = {6},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3665348.3665366,
author = {Fang, Huilin and Zhou, Zhiguo},
title = {Research on acoustic Model of Putian Dialect Speech Recognition Based on Deep Learning},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665366},
doi = {10.1145/3665348.3665366},
abstract = {The Putian dialect is an ancient language with a rich historical and cultural background. Currently, there is a lack of published research on automatic speech recognition (ASR) technology for the Putian dialect due to various reasons. One notable example is the absence of an audio corpus specifically for this dialect. Moreover, the pronunciation of Putian dialect significantly differs from that of Mandarin used in our daily lives. This paper aims to address these challenges by collecting 8000 isolated words from a phonetic corpus dedicated to the Putian dialect. Based on this self-built database, I attempt to achieve speech recognition for the Putian dialect using various models such as single phoneme model recognition, three-phoneme model, linear discriminant analysis, maximum likelihood change, speaker adaptive training, quick model and DNN-HMM model. Notably, the DNN-HMM-2 model yields the best results with a word error rate of 20.56\%. In this experiment, I introduce an additional layer of mapping in order to construct a two-step mapping method for phoneme - Putian dialect - Mandarin alignment. This approach improves training alignment accuracy and reduces character error rate by approximately 3\% compared to using only one-level mapping.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {94–99},
numpages = {6},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3665348.3665394,
author = {Li, Zhengle and Pan, Ying and Zhang, Tonglin and Zhou, Yan and Huang, Xinying and Chai, Xiaolong},
title = {Analysis of Infringement Determination Methods in Personal Information Protection Disputes Based on Machine Learning},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665394},
doi = {10.1145/3665348.3665394},
abstract = {The development of big data provides abundant resources for enterprises to target users and adjust development strategies. However, it also brings about an escalating risk of personal information leakage. In this context, the methods and standards for determining the acts of infringement have yet to be clarified, leading to different judgements in the same case. Grounded on the existing modes of legislation and judicial adjudication, the paper proposes machine learning methods to deconstruct the determination of acts of infringement, with a goal of assisting judges in making more accurate judgments. Specifically, the paper employs Adaboost regression model and the logistic regression model with gradient descent. Adaboost regression model combines multiple weak classifiers to improve the accuracy of classification, while the logistic regression model with gradient descent adjusts model values by minimizing the loss function to pursue the optimal ones. Through training and evaluation, the paper reveals that these models possess favorable applicability in determining the acts of infringement. Moreover, the paper also mentions the predictive results and evaluation metrics of the models, as well as their validation and application under various determination methods.Through the application of mathematical models and machine learning algorithms, the paper deconstructs the methods of infringement determination in personal information protection disputes. The evaluation and analysis of the models' applicability provides a guarantee for its adaption to different methods of determination, ensuring a reliable algorithmic model for the judicial review of personal information protection disputes when applying machine learning.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {266–273},
numpages = {8},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3665348.3665354,
author = {Niu, Lianqiang and Bao, Han},
title = {Fast Tactile Paving Segmentation Model Based on Reparameterized Structure},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665354},
doi = {10.1145/3665348.3665354},
abstract = {In order to solve the problem of low segmentation accuracy due to the interference of noise information such as light and shadow in the current lightweight blind segmentation model, we propose a fast tactile paving segmentation model based on reparameterize. The multi-branch training structure learns spatial features such as tactile paving, light and shadow while compensating for the loss of information in the feature map due to downsampling operations as well as enhancing the learning of its features in the channel dimension. The single-branch structure obtained after reparameterization of the multibranch structure ensures faster inference of the model as well as lower memory access costs. The introduction of coordinate attention allows the model to learn the distribution law of the features of tactile paving, light and shadow, etc. in the spatial dimension and automatically generate the attention map with weight information, effectively reducing the weight of the spatial structural features of noise such as light and shadow. Experiments show that the model's ability to resist interference from environmental noise such as light and shadow has been enhanced and the segmentation accuracy has been further improved under the premise of ensuring the inference speed.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {24–28},
numpages = {5},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3665348.3665353,
author = {Wang, Shen and Cai, Fei and Zhang, Mengxi},
title = {Multilingual Event Causality Identification via Meta-learning with Knowledge},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665353},
doi = {10.1145/3665348.3665353},
abstract = {Multilingual Event Causality Identification (Multilingual ECI) is the task of detecting causal relations between events mentioned in the multilingual text. The biggest challenge of this task is the small size of the minor language corpus, making it very difficult to train a model with good performance. To address this issue, we propose a meta-learning method that integrates multilingual knowledge, which mainly includes two modules: the multilingual knowledge obtain module, which alleviates data scarcity by introducing multilingual knowledge from outside; The meta-learning module utilizes ProtoMAML combined with l₂ standardization to extract language independent causal features from text and learn better event representations in different languages. The experimental results on the latest publicly available MECI dataset show that our model outperforms the current best baseline, with an average F1 improvement of 3.9\% in different languages.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {18–23},
numpages = {6},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3665348.3665400,
author = {Yuan, Lin and Feng, Zhishang},
title = {A dynamic key update scheme for UAV clusters in denied environments},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665400},
doi = {10.1145/3665348.3665400},
abstract = {Unmanned aerial vehicles (UAV) are susceptible to unsecure communication problems in denied environments because of strong interferences from physical fields and electromagnetic fields. A dynamic key management scheme based on Chinese remainder theorem (CRT) and the Hash function is proposed here to solve this problem. Specifically, as changes in the topological structure and unstable communication are likely to cause security problems in communication of drones in the battlefield, two dynamic key update protocols, i.e., topology-aware key update and mutual-healing key update, are put forth here to ensure secure communication through dynamically-updated keys. Analyses reveal that the proposed scheme has forward and backward security and resistance to collusion attacks and replay attacks. Meanwhile, the proposed method has been compared with existing methods through tests on the popular MIRACL dataset by Visual Studio 2019, and the test results show that our method optimizes the computing performance and the communication overhead, and achieves higher updating efficiency.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {303–312},
numpages = {10},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3665348.3665406,
author = {Gao, Guanghui and Deng, Dapeng and Shi, Gang and Guo, Yining and Ning, Zhaole},
title = {Research on Methodology of Intelligent Traffic Accident Detection based on Enhanced YOLOv8 Algorithm},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665406},
doi = {10.1145/3665348.3665406},
abstract = {In response to the problem of complex visual feature categories and sparse datasets for related refined categories in traffic accidents, this article has created a traffic accident target detection dataset, the AccOD dataset, which is divided into five categories. This dataset can serve as a benchmark for training and evaluating traffic accident target detection algorithms. Aiming at the real-time, accuracy, and intelligence issues of traffic accident detection, an improved traffic accident target detection network based on YOLOv8 is proposed. This network replaces a more efficient backbone network structure and improves the downsampling module that integrates different scale features in the head network, reducing the number of model parameters and computational complexity while improving the detection accuracy of the model. The C2f module is improved to balance various contextual information, further enhancing the detection ability of the model. The experiment showed that the improved model achieved a 4.75\% improvement in mAP0.5 and 4.58\% improvement in mAP0.5:0.95 on the AccOD dataset compared to YOLOv8n, and a 2.14\% improvement in mAP0.5 and 6.23\% improvement in mAP0.5:0.95 on the open source ACD dataset. This algorithm improves the detection accuracy of traffic accidents and maintains the lightweight of the model, providing an effective method for intelligent traffic accident detection.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {340–348},
numpages = {9},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3665348.3665385,
author = {Yang, Lang},
title = {A meta-learning based method for detecting malicious traffic with few-shot},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665385},
doi = {10.1145/3665348.3665385},
abstract = {For the increasingly complex network environment, new types of attacks are emerging, and the limited attack samples may not be sufficient for traditional detection methods to achieve good detection results. In this regard, this paper proposes a few-shot detection method based on meta-learning, which mainly consists of three parts: data preprocessing, feature extraction and comparison, and classification and detection. The original data samples are transformed into grey scale graphs, and after combining the samples two by two, they are respectively subjected to feature extraction and comparison, which results in similarity scores, which are finally used to carry out classification. It is shown through experiments that the accuracy of detection can reach up to 98.45\% in the case of few-shot.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {211–216},
numpages = {6},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3641237.3691672,
author = {Thominet, Luke and Acosta, Kristine and Amorim, Jacqueline and Sohan, Vanessa Kraemer},
title = {How Our AI-assisted Qualitative Analysis Failed},
year = {2024},
isbn = {9798400705199},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3641237.3691672},
doi = {10.1145/3641237.3691672},
abstract = {The authors describe how their Generative Artificial Intelligence (GAI)-assisted qualitative research project failed to produce publishable results. Based on this experience, they argue for the value of embracing and reflecting on failure in GAI-assisted qualitative research. To frame this argument, they draw on two theories of generative failure: failing forward, which uses failures to iterate on designs to meet existing criteria, and failing sideways, which reconsiders the criteria for success. Using a fail-forward perspective, the authors describe how they might revise their research methods for data preparation, process documentation, and task delegation to create more reliable results. Then, using a fail-sideways perspective, they reexamine criteria for publishable results to reimagine the study more fundamentally.},
booktitle = {Proceedings of the 42nd ACM International Conference on Design of Communication},
pages = {212–216},
numpages = {5},
keywords = {Generative Artificial Intelligence, Generative Failure, Research Methods},
location = {Fairfax, VA, USA},
series = {SIGDOC '24}
}

@inproceedings{10.1145/3665348.3665409,
author = {Liu, Bao and Wang, Fengran},
title = {Enhanced Bearing Fault Diagnosis under Strong Noise: An improved Inception Inverted Residual Ghost ShuffleNet},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665409},
doi = {10.1145/3665348.3665409},
abstract = {As an indispensable and easily damaged part in large equipment, bearings were of great significance for their rapid and accurate fault diagnosis. The working environment of bearings was usually harsh and complicated. In order to improve the accuracy of bearing fault diagnosis under strong noise, an improved IRGShuffleNet network structure was proposed. The initial multi-scale structure based on LIBP was innovatively used to extract the shallow feature information of the network. Features were extracted from high-dimensional data using the inverted residual structure, and redundant feature maps were generated in a cheaper way by Ghost convolution. Random depth structure and flooding regularization were used to prevent overfitting of the model. To evaluate the validity of the proposed model, a series of comparative experiments were conducted. Compared with the standard residual network, the accuracy of the model proposed in this paper was improved by 5.90\% and 9.72\% respectively under -4dB and -10dB noise. The model parameter was reduced from 25.7M to 1.98M, FLOPs were reduced by about 75.2\%, and other performance indicators were also improved at different amplitudes. These results showed that the proposed model had good diagnostic adaptability.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {358–363},
numpages = {6},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inbook{10.1145/3728725.3728728,
author = {Yang, Kai and Shi, Huaifeng and Wang, Rui and Zhou, Lei and Peng, Jiahui and Liu, Chaofan},
title = {Trend Decomposition Enhanced Large Language Model for Non-Stationary Network Traffic Prediction},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728728},
abstract = {Regular network traffic prediction techniques, which generally rely on data normalization, often fail to account for the non-stable characteristics of network traffic, leading to a decline in estimate reliability. A novel approach to this problem is the Trend Decomposition Enhanced Large Language Model for Non-Static Network Traffic Prediction (TD-LLM). Applying trend decomposition as the preparation approach, the proposed strategy first divides raw information into resolved and non-fixed parts. The predetermined portion is standardized and processed, implementing a multi-head cross-attention mechanism, while a better self-attention mechanism improves the non-stationary element. Prompt engineering is used to create specially designed causes that serve as a guideline for the design throughout the forecast process. Finally, the processed data and creates are integrated into the large language model to produce the predicted results. In contrast to foundation models like Time- LLM, LSTM, Informer, and Transformer, the TD-LLM type drastically reduces mean squared error for network traffic prediction across many regions, according to exploratory evaluations, with decreases of 19.73 \%, 54.71 \%, 76.88 \%, and 61.48 \%, respectively.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {12–19},
numpages = {8}
}

@inproceedings{10.1145/3665348.3665413,
author = {Zhang, Xuyang and Ni, Li and Zhu, Hang and Shen, Jialu and Wang, Lei},
title = {Research on intelligent operation and maintenance system of distributed photovoltaic power station based on Internet of Things technology},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665413},
doi = {10.1145/3665348.3665413},
abstract = {With the rapid development of renewable energy, especially solar energy, distributed photovoltaic power plants have become a crucial component of energy transition. In order to improve the operational efficiency and reduce maintenance costs of photovoltaic power plants, this paper proposes an IoT-based intelligent operation and maintenance system for distributed photovoltaic power plants. The system integrates advanced sensor networks, cloud computing, big data analysis, and artificial intelligence technologies to achieve real-time monitoring, fault prediction, performance optimization, and automated maintenance decision-making for photovoltaic power plants. Through a series of experiments, this paper validates the effectiveness of the proposed system in improving the operational efficiency of photovoltaic power plants, ensuring stable system operation, and optimizing energy output. Furthermore, this research explores the application challenges and strategies of IoT technology in the operation and maintenance of photovoltaic power plants, providing new perspectives and methods for the intelligent management and maintenance of photovoltaic power plants.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {380–387},
numpages = {8},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3665348.3665417,
author = {Qi, Ji and Li, Xiujun and Zhang, Hang and Meng, Tianyu},
title = {Research on Multi-feature EEG Emotion Recognition Method Based on 1D-Inception},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665417},
doi = {10.1145/3665348.3665417},
abstract = {In addressing the limited receptive field of single-layer convolution in EEG emotion recognition, the need to stack multiple layers of convolution for expanding the receptive field poses challenges of increased parameter size and training cost. Simultaneously, there is an oversight of channel information in EEG emotion recognition. To tackle these issues, this paper introduces a multi-feature EEG emotion recognition model based on 1D-Inception (1D-Inception-Channel-Spatial-LSTM Network, DCSL). DCSL integrates channel information, spatial features, and temporal dependencies, resulting in more effective EEG emotion recognition. The Inception structure is enhanced to fit EEG signals, enabling multi-scale convolution. The Channel Attention Module is introduced to learn the weights of different channels, enhancing emotion recognition performance. Additionally, a Spatial Attention mechanism is incorporated to further extract spatial features from EEG signals. To better capture the temporal dependencies in EEG signals, an LSTM module is introduced at the end of the model. Experimental results confirm the effectiveness of the DCSL network in EEG emotion recognition tasks.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {407–411},
numpages = {5},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3665348.3665392,
author = {Sun, Zhipeng and Zeng, Guosun},
title = {A Two-stage Coarsening Method for a Streaming Graph with Preserving Key Features},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665392},
doi = {10.1145/3665348.3665392},
abstract = {With the rapid development of 5G and mobile internet, various application fields continue to generate diverse dynamic streaming graphs with large scales and complex structures. Analyzing, querying and mining such a streaming graph face great challenges. To address the challenge of low query efficiency and avoid data loss, this paper proposes a two-stage coarsening method that preserves key features of a streaming graph while improves query efficiency. In the initial stage, the size of the streaming graph is not that large yet. To preserve its structure and attributes as much as possible, a linear mapping method is used for graph coarsening. As the size of the streaming graph increases, a graph neural network model based on cluster pooling is constructed in the second stage. Using a large number of graph coarsening results obtained in the initial stage to train and optimize this model. Then, we use this optimized model to obtain the subsequent summaries, which achieves online efficient graph coarsening. Finally, extensive experiments indicate that our algorithm significantly improves the quality of streaming graph coarsening compared to traditional algorithms. Even compared to the state-of-the-art algorithm Auxo, our algorithm shows a better performance on preserving key features.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {253–260},
numpages = {8},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3665348.3665408,
author = {Liu, Bao and Qi, Zhi},
title = {Extracting features for enhanced air quality prediction: a self-tuning spatio-temporal residual network approach fusing adaptive adjacency matrix and attention mechanism},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665408},
doi = {10.1145/3665348.3665408},
abstract = {The Air Quality Index (AQI) is an effective measure of air quality, and accurate prediction of the AQI is important for air pollution control and the daily travel of people. Air pollutants have strong spatio-temporal correlation, and the current air quality prediction methods often fail to adequately extract the spatio-temporal features in the data, which can cause prediction accuracy problems. This paper proposed a Self-Tuning Spatio-Temporal Residual Network (AAM-AST2RN) fusing adaptive adjacency matrix and attention mechanism for enhanced air quality prediction. AAM-AST2RN utilized a parallel network structure, which was capable of fully extracting and fusing spatio-temporal features and automatically adjusting the network hyperparameters. In addition, the AAM-AST2RN model constructed the AAM generation module, residual connection and Improved Channel Attention mechanism (ICA), the AAM generation module avoided the adverse effect of static adjacency matrix on the model feature extraction, the residual connection and ICA effectively alleviated the node feature over-smoothing problem caused by Graph Convolutional neural Network (GCN) in the model and improved the node feature differentiation. In order to validate the model effect, the model was utilized to predict the AQI of 12 air monitoring sites in Beijing, and compared with the prediction effect of Convolutional Long Short-Term Memory neural network (ConvLSTM), combined Convolutional Neural Network and Long Short-Term Memory neural network (CNN-LSTM), and GCN-LSTM. Compared with the above three spatio-temporal prediction models, the coefficient of determination (R2) of the AAM-AST2RN model was improved by 11.30\%, 1.95\%, and 1.54\%, respectively, and the prediction error was lower. The AAM-AST2RN air quality prediction model proposed in this paper can fully extract the spatio-temporal features in the data and has a good prediction performance for air quality.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {353–357},
numpages = {5},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3665348.3665411,
author = {Li, Hong},
title = {Research and application of network analysis algorithms based on underground pipeline networks},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665411},
doi = {10.1145/3665348.3665411},
abstract = {With the deepening application of underground pipeline GIS system, various graphical data are becoming increasingly rich and accurate. The demand for network analysis functions such as path analysis, resource allocation, and connectivity analysis is rapidly increasing both domestically and internationally. Various network analysis algorithms based on GIS urgently need industrial level applications. We have established a three-dimensional network model of water supply network based on GIS technology, and created a spatial database to store graphic data and study the effectiveness of network analysis algorithms through underground pipeline network business data. We have designed optimal algorithmic solutions for various complex underground pipeline problems, such as burst pipe problems, shortest path problems, and maximum flow problems in the water supply network. This solution can be applied to various pipeline networks and it has strong application value and economic value.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {370–374},
numpages = {5},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3665348.3665404,
author = {Du, Peng and Song, Xudong},
title = {Lightweight Target Detection: An Improved YOLOv8 for Small Target Defect Detection on Printed Circuit Boards},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665404},
doi = {10.1145/3665348.3665404},
abstract = {Addressing the performance and efficiency challenges of detecting small target defects on PCB (printed circuit board), this research proposes a lightweight inspection model for YOLOv8-based. The initial step involves employing an enhanced Efficient_Detect network architecture with the aim of diminishing the volume of model parameters and computational intricacy, while simultaneously augmenting resource utilization. Then, the deformable convolutional networks v2 (DCNv2) are integrated into the C2f structure to improve feature extraction for defects with diverse shapes on the PCB surface, enhancing the model's capability to identify different categories of flaws. Meanwhile, the Inner-SIoU loss function is utilized instead of the conventional CIoU loss function to enhance both the precision of inspection and the robustness of the model. The enhanced approach achieves an mAP50 of 96.8\% and an mAP50-95 of 72.2\%, marking an improvement of 3.9\% and 10.1\%, respectively, comparison with base YOLOv8n model. In addition, the parameter counts of the model, GFLOPs, and weight size are notably reduced by 18.4\%, 33.3\%, and 19.7\%, respectively, resulting in final metrics of 2.34 M, 5.4 GFLOPs, and a weight size of 4.9 M. In addition, the detection speed can reach up to 161 FPS, which has the potential for real-time detection. It provides an efficient and lightweight PCB small target defect detection solution for scenarios with limited computing resources.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {329–334},
numpages = {6},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3665348.3665389,
author = {Zhang, Jian and Liu, Shengquan and Liu, Zhihua},
title = {Research on APT Malware Detection Based on BERT-Transformer-TextCNN Modeling},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665389},
doi = {10.1145/3665348.3665389},
abstract = {In recent years, with the development of the Internet, APT malware detection remains an important issue for society. The lengthy nature of API sequence text leads to polysemy issues with distant repeated words. Traditional deep learning models must improve extracting the long-term dependencies of entire API sequences. During an APT attack, attackers combine benign and malicious sequences to conceal their malicious intent, resulting in localized malignancy. In response to these issues, the paper proposes A BERT-Transformer-TextCNN model for attack detection. BERT encodes API sequences, addressing the polysemy issue of distant repeated words. The Transformer-encoder extracts long-term dependencies, resolving the global feature issues of long sequences, and TextCNN is applied to capture local sequence information to solve the problem of localized malignancy. Based on actual APT malware samples, BERT encoding achieves higher precision than traditional encoders. Employing a Transformer-Encoder to extract long-term dependencies yields higher precision than using BiLSTM to extract long-term relationships. When adding features related to local feature correlation, the precision increased by 3.39\%.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {235–242},
numpages = {8},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3665348.3665379,
author = {Wang, Wei and Lin, Wenbin and Lv, Hang},
title = {Applying DMC of Segmentation Strategy of Reward in Military Chess Game},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665379},
doi = {10.1145/3665348.3665379},
abstract = {Machine game is divided into complete information game and incomplete information game. Similarly, according to the transparency of opponent information, military chess game can be divided into complete information chess game and incomplete information chess game. In this paper, we mainly study the complete information chess game. Aiming at the high time complexity and high computing power of traditional tree search algorithms, a Deep Monte-Carlo (DMC) algorithm based on segmentation strategy of reward is proposed to solve the problem of high mortality of high-level chess pieces and improve the winning rate of the whole game system. By competing with military chess systems constructed by Upper Confidence Tree (UCT) algorithm and Deep Q-learning (DQN) algorithm, we constructed a complete verification scheme to obtain the best segmentation strategy of reward and verify the effectiveness of DMC algorithm based on segmentation strategy of reward.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {173–178},
numpages = {6},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inbook{10.1145/3728725.3728742,
author = {Zhang, Xiaofeng and Gao, Xianming and Tao, Peilin and Feng, Tao},
title = {GraphSynth: Synthesis of Network Configuration Templates Using Large Language Models},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728742},
abstract = {We investigate whether Large Language Models can synthesize correct network configuration templates in a simpler and more efficient way with limited prompts. The experimental results demonstrate that the LLM's shortcomings in command relation understanding can be supplemented by external models to generate promising templates. We do not rule out the contribution of validators to property-specific consistency. However, our framework aims to preserve the large language model generation capability while focusing on the implicit relationships in configurations. By introducing a hierarchical command graph, we follow a top-down matching to generate templates, reducing feedback interactions in generation and bring potential solutions for automated provisioning. We process template generation under different scenarios, including establishing BGP basic connections on multiple routers and imposing restrictions such as route redistribution at specific locations. Human involvement is restricted to providing critical information. Experiment shows that expected templates can be generated in the first iteration, particularly for the basic task. The advanced task is influenced by multiple factors, however, it still shows considerable potential in terms of coverage with possibly feasible templates.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {108–114},
numpages = {7}
}

@inproceedings{10.1145/3734921.3734934,
author = {Wang, Shuo and Lu, Yuqi and Luo, Ruijue and Li, Boxuan and Zhang, Yingjie and Xiao, Liang},
title = {A BERT-based sentiment analysis model for depressive text},
year = {2025},
isbn = {9798400713460},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3734921.3734934},
doi = {10.1145/3734921.3734934},
abstract = {Depression is a big public health problem around the world. Like the traditional methods of screening, clinical assessments, and self-rating scales are among the limits. These methods are based on the subjectivity of patient reports and the experience of doctors and clearly can have bias. In addition, they do not permit real-time monitoring of changes in mood. For this reason, this field of mental health is challenged with accurately and efficiently detecting probable mood changes in individuals at risk for depression. To resolve these challenges, we then propose a BERT-based sentimental analysis model adapted for depression-related text. Current methods for screening for traditional depression and established sentiment analysis models are hindered by data scarcity, data imbalance, and the problems of deriving emotional changes in a dynamic and clean manner. To leverage the strengths of BERTs in our model, we train the last layer of BERTs and then construct a classification layer tailored to the depressive emotion, which is more sensitive to the semantic information of the mood. Moreover, we also present data augmentation in its simplest form, which is the rearrangement of positive samples to increase the diversity of samples while still preserving the semantic meaning. To tackle data imbalance, we introduce a dynamic weight mechanism to the BCEWithLogitsLoss function by adjusting the weight of the positive samples with data distribution. The model effectively captures the complex emotional changes by combining global and local semantic dynamic representations. In our empirical study, we analyze the open-source dataset of interview transcripts of 162 volunteers. Our model is evaluated using the experimental results, and the precision, the recall rate, and the F1 score obtained are 0.667, 0.667, and 0.667, respectively. Our model has achieved 2.62 higher in terms of precision and F1 score as compared to the existing models. The results indicate that our model provides a big benefit in identifying screened individuals at risk for depression.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence and Digital Media},
pages = {76–83},
numpages = {8},
keywords = {BERT model, Depression screening, medical text processing, sentiment analysis, text classification},
location = {
},
series = {GADM '25}
}

@inproceedings{10.1145/3734921.3734930,
author = {Wei, Han and Qu, Dan and Zhang, Wenlin and Niu, Tong and Peng, Sisi and Guo, Zhigang},
title = {Recommendation Algorithm Based on Large Language Model: Consideration on Algorithm Governance and User Rights Protection},
year = {2025},
isbn = {9798400713460},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3734921.3734930},
doi = {10.1145/3734921.3734930},
abstract = {With the rapid development of big data and artificial intelligence technology, recommendation systems play a crucial role in improving user experience, increasing platform revenue, and optimizing content distribution. However, the wide application of recommendation system also brings about problems such as filter bubble, algorithm black box, labor rights protection, the big data-enabled price discrimination and so on. By processing massive data, the large language model can accurately construct and predict user interests and preferences, process and generate diverse content, thus improving the relevance and fairness of recommendation, enhancing the diversity and transparency of recommendation system, protecting the rights and interests of workers, preventing price discrimination, and realizing algorithm governance and the protection of user rights and interests. This paper designs a recommendation algorithm framework based on a large language model, aiming to build a fair and transparent recommendation system, which can reduce information bias, improve the transparency of the list, protect the rights and interests of workers, prevent price discrimination, and promote social equity.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence and Digital Media},
pages = {52–57},
numpages = {6},
keywords = {Algorithm governance, Large language model, Recommendation algorithm, User rights and interests},
location = {
},
series = {GADM '25}
}

@inproceedings{10.1145/3665348.3665357,
author = {Wu, Junying and Lu, Yanyan and Li, Zixin and Peng, Jiao and Xu, Xing and Song, Hui},
title = {Word-Phrase Fusion Encoding Model for Natural Language Understanding in the Electric Power Field},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665357},
doi = {10.1145/3665348.3665357},
abstract = {Intelligent question-answering systems have been widely applied in the field of electric power. However, with the complexity of terms in the electric power field, the generic word-based encoding approach of natural language understanding model fails to identify those domain phrases even if they appear in the training samples. To improve the semantic accuracy of special content labeling, this paper proposes a words-phrase fusion encoding NLU model with the help of domain corpus. We pre-train a phrase-level Bert model in the electric power field which is involved during the model encoding step to accurately capture the semantics of domain terms and perceive the boundary of phrases. Additionally, continuous consistency loss of sequences is added to the model to reduce the to reduce the misclassification of individual words in the phrases. Experiment with real dataset in power QA system demonstrates that our model helps improve semantic parsing accuracy on both training and untrained items, alleviates the complete dependency of the phrase encoding model on word segmentation results.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {43–49},
numpages = {7},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inbook{10.1145/3728725.3728821,
author = {Cai, Litao and Liu, Mingmei and Feng, Youbing and Xiong, Feng},
title = {Research on fall detection algorithm based on improved YOLOv8},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728821},
abstract = {Aiming at the traditional fall detection algorithms that suffer from low detection accuracy and poor real-time performance in dealing with small targets and multi-scale features in complex scenes, this study proposes a fall detection algorithm based on an improved YOLOv8 model. First, the SCConv module is utilized to combine with the C2f module in the YOLOv8 backbone network to construct the C2fSC module, which enhances the feature extraction capability of the model while reducing the computational complexity. Second, the SPPF module is integrated with the BAM attention mechanism to weaken the influence of background interference. Finally, WIoU is used to replace the original loss function to optimize the positioning accuracy of the bounding box and enhance the overall performance of the model. The experimental results show that compared with the original YOLOv8 model, the improved model improves 4.9\%, 4.2\%, and 4.4\% in P, R, and mAP metrics, respectively, and the number of parameters and FLOPs are only 2.8M and 7.7G, which proves that the improved model has excellent robustness and effectiveness.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {610–615},
numpages = {6}
}

@inbook{10.1145/3728725.3728752,
author = {Xiong, Botao},
title = {Audio-driven Talking-face Synthesis based on 3D Gaussian},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728752},
abstract = {Audio-driven talking-face synthesis is of significant importance in various application scenarios, such as remote meetings, AR/VR, and digital humans. Currently, the work in this field can be broadly divided into two categories: implicit representation-based methods and explicit representation-based methods. Implicit representation-based methods often use neural network models to represent human face. For example, NeRF (Neural Radiance Field) uses MLP (Multi-Layer Perceptron) to represent human face, and by inputting the camera pose, NeRF renders a face image from that viewpoint. When training with audio features, it can render face images with different facial movements. However, due to the lack of editability and slow rendering speed of implicit representations, the industry has been exploring new representation methods. Explicit representation-based methods commonly use 3D Gaussian representations of the face, which offers high editability and nearly real-time rendering speed, making these methods surpass implicit-based approaches in many metrics. However, it is difficult to incorporate other signals during the rendering optimization process. To address this issue, this paper combines 3D Gaussian representations with the 3D head model FLAME (Faces Learned with an Articulated Model and Expressions). The FLAME model represents the face's shape and facial movements with a set of parameters. In this paper, audio signals are first transformed into parameters representing facial movements, which are used to initialize the 3D model generated by the parameters, followed by rendering optimization. This paper proposes a 3D Gaussian-based audio-driven lip-sync video generation system. It consists of two main modules: the audio-to-facial movement module and the 3D Gaussian rendering module. The audio-to-facial movement module uses the extracted audio features to generate parameters that control the FLAME model, converting the audio input into natural and reasonable lip shapes and facial movements. The 3D Gaussian rendering module initializes the 3D Gaussian with the FLAME model, optimizes the rendered image from coarse to fine, and finally stitches the generated images together to create a continuous video. Experiments show that this system can generate lip-synchronized, accurate, high-fidelity videos.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {171–177},
numpages = {7}
}

@inbook{10.1145/3728725.3728727,
author = {Li, Hao and Zhao, Jiahui and Gu, Daixuan},
title = {HoloDiT: Exploring 3D Diffusion Transformer Models Using 2D Images},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728727},
abstract = {Recently, Transformer-based diffusion models have produced impressive results in 3D shape generation. However, at this stage, they can only be trained using 3D point cloud data to generate simple 3D shapes based on point cloud representations. Building on this, we propose HoloDiT. It uses a set of real-world 2D images and corresponding camera poses as input, generating 3D auxiliary samples that are fed into a Transformer-based 3D diffusion model for training in 3D shape generation. By training the rendering function to generate color and density, we can restore the color details of object surfaces. This enables HoloDiT to generate realistic 3D shapes from 2D images. We trained on the real-world CO3Dv2 dataset, and the generated results demonstrate competitive quality and consistency compared to existing 3D generation models.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {7–11},
numpages = {5}
}

@inproceedings{10.1145/3759275.3759292,
author = {Hao, Xiaoyan and Cui, Yingjie},
title = {Smart Museums, Smarter Experiences: Exploring the Impact of GenAI Usability on Digital Heritage Enjoyment},
year = {2025},
isbn = {9798400719554},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3759275.3759292},
doi = {10.1145/3759275.3759292},
abstract = {With the increasing integration of generative artificial intelligence (GenAI) into cultural heritage dissemination, users' digital interactions in museums have evolved from passive information reception to active co-creation. To investigate the mechanisms through which GenAI usability influences user enjoyment, this study applies the Hedonic Motivation System Adoption Model (HMSAM) and develops a theoretical framework incorporating six mediating constructs: perceived ease of use, perceived usefulness, curiosity, joy, sense of control, and immersion. Based on 582 valid responses collected from the “Cloud Tour of Dunhuang” digital heritage initiative, structural equation modeling was conducted using SmartPLS 4.0. The findings reveal that GenAI usability does not directly affect users' enjoyment; instead, its influence is transmitted indirectly through several psychological mediators. Specifically, GenAI usability significantly enhances perceived ease of use, usefulness, curiosity, joy, and control, each of which contributes to users’ enjoyment, with joy demonstrating the strongest mediating effect. Although GenAI usability also increases users’ sense of immersion, immersion itself does not significantly predict enjoyment. These results extend the applicability of HMSAM in the cultural technology domain, deepen the theoretical understanding of user behavior in smart museums, and offer practical guidance for optimizing GenAI-based interactive system design in cultural heritage contexts.},
booktitle = {Proceedings of the 2025 2nd International Conference on Digital Systems and Design Innovation},
pages = {107–117},
numpages = {11},
keywords = {Cultural Heritage Technology, Generative Artificial Intelligence, Hedonic Motivation, Smart Museums, Usability, User Enjoyment},
location = {
},
series = {ICDSDI '25}
}

@inproceedings{10.1145/3706599.3719972,
author = {Lin, Xin and Han, Xiaonan and Qiu, Junqiao},
title = {Generative AI in Special Education: Teachers' Insights on Instructional Enrichment vs. Accommodations},
year = {2025},
isbn = {9798400713958},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3706599.3719972},
doi = {10.1145/3706599.3719972},
abstract = {Generative Artificial Intelligence (GAI) holds significant potential for transforming education, yet its adoption in special education remains underexplored. It is important to investigate how special education teachers perceive and use GAI for two distinct instructional tasks: instructional enrichment and accommodations. Although the Technology Acceptance Model (TAM) has been extensively used to study technology adoption, its application to GAI in special education is underrepresented. This gap is critical as special education teachers must address diverse needs of students with multiple disabilities. Moreover, limited research had examined how TAM dimensions vary across different tasks. This late-breaking study revealed that teachers consistently rated GAI lower for accommodations across all TAM dimensions, with particularly differences in Intention to Use and Actual Use. The results highlighted the complexities of adopting GAI for distinct instructional purposes. Although GAI demonstrated potential for supporting instructional enrichment, its inability to generate individualized recommendations limited its effectiveness for accommodations.},
booktitle = {Proceedings of the Extended Abstracts of the CHI Conference on Human Factors in Computing Systems},
articleno = {304},
numpages = {6},
keywords = {Generative Artificial Intelligence, Special Education, Technology Acceptance Model, Instructional Enrichment, Instructional Accommodations},
location = {
},
series = {CHI EA '25}
}

@inproceedings{10.1145/3716640.3716658,
author = {Feng, Tony Haoran and Luxton-Reilly, Andrew and W\"{u}nsche, Burkhard C and Denny, Paul},
title = {From Automation to Cognition: Redefining the Roles of Educators and Generative AI in Computing Education},
year = {2025},
isbn = {9798400714252},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3716640.3716658},
doi = {10.1145/3716640.3716658},
abstract = {Generative Artificial Intelligence (GenAI) offers numerous opportunities to revolutionise teaching and learning in Computing Education (CE). However, educators have expressed concerns that students may over-rely on GenAI and use these tools to generate solutions without engaging in the learning process. While substantial research has explored GenAI use in CE, and many Computer Science (CS) educators have expressed their opinions and suggestions on the subject, there remains little consensus on implementing curricula and assessment changes.In this paper, we describe our experiences with using GenAI in CS-focused educational settings and the changes we have implemented accordingly in our teaching in recent years since the popularisation of GenAI. From our experiences, we propose two primary actions for the CE community: 1) redesign take-home assignments to incorporate GenAI use and assess students on their process of using GenAI to solve a task rather than simply on the final product; 2) redefine the role of educators to emphasise metacognitive aspects of learning, such as critical thinking and self-evaluation. This paper presents and discusses these stances and outlines several practical methods to implement these strategies in CS classrooms. Then, we advocate for more research addressing the concrete impacts of GenAI on CE, especially those evaluating the validity and effectiveness of new teaching practices.},
booktitle = {Proceedings of the 27th Australasian Computing Education Conference},
pages = {164–171},
numpages = {8},
keywords = {Generative Artificial Intelligence, GenAI, Strategy, Assignments, Metacognition, Assessments},
location = {
},
series = {ACE '25}
}

@inbook{10.1145/3728725.3728734,
author = {Wang, Xiaoyu and Zhao, Weidong and Qiu, Liqing},
title = {Building Specialized Adversarial Examples for Improved Spelling Error Correction},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728734},
abstract = {Pre-trained language models based on deep neural networks have achieved superior performance in Chinese spelling correction. However, these models are heavily dependent on the size and quality of the training corpus and exhibit poor performance when faced with unknown errors. Inspired by adversarial learning, this paper focuses on Chinese character input scenarios and propose an effective attack strategy to generate more valuable training examples in these scenarios. This strategy jointly utilizes the attention mechanism and the logit output to determine the attack position and generates misspelling instances in a perturbed pinyin manner. The generated adversarial samples are then gradually added to the training set to train the model. Further, this paper explores the effect of the proportion of samples in the adversarial training process on model performance. The effectiveness of the proposed strategy are demonstrated on three different architectural baseline models.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {50–57},
numpages = {8}
}

@inproceedings{10.1145/3716489.3728431,
author = {Mirindi, Derrick and Sinkhonde, David and Mirindi, Frederic and Bezabith, Tajebe},
title = {Advanced evaluation of BIM-GenAI using OpenAI o1 and ethical considerations},
year = {2025},
isbn = {9798400714979},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3716489.3728431},
doi = {10.1145/3716489.3728431},
abstract = {The rapid advancement of artificial intelligence (AI) has led the AI community to speculate that artificial superintelligence (ASI) may be within reach, particularly if an AI system can iteratively search for solutions, learn from results, and leverage improved knowledge for more searches. In this context, this study explores the integration of Generative Artificial Intelligence (GenAI) into Building Information Modeling (BIM) by focusing on the four key pillars of the OpenAI o1 model and their ethical implications. Through comprehensive analysis of existing literature, we examine these pillars—policy initialization, reward design, search strategies, and learning mechanisms—and their application in BIM-GenAI within a continuous improvement cycle. Results demonstrate that policy initialization generates human-like reasoning behaviors and domain-specific knowledge for BIM tasks. Reward design, central to reinforcement learning, optimizes BIM objectives through measurable metrics and learned evaluation methods. Search strategies prove valuable for exploring complex design spaces and generating high-quality BIM solutions, while learning mechanisms, including policy gradient and behavior cloning, enable continuous model improvement through feedback. The study emphasizes the importance of establishing BIM-AI protocols, maintaining human expertise in decision-making, and balancing automation with human input. Our findings suggest that while GenAI, powered by reinforcement learning, offers significant potential for enhancing BIM capabilities, three critical ethical considerations—data privacy and security, algorithmic bias mitigation, and transparency and accountability—must guide responsible implementation. This research contributes to the growing body of knowledge on AI in construction technologies and provides a foundation for the ethical advancement of BIM-GenAI systems using OpenAI o1.},
booktitle = {Proceedings of the 2025 Computers and People Research Conference},
articleno = {1},
numpages = {8},
keywords = {Artificial Intelligence, Building Information Modeling, Generative Artificial Intelligence, OpenAI o1},
location = {
},
series = {SIGMIS-CPR '25}
}

@inbook{10.1145/3728725.3728828,
author = {Xu, Xintao},
title = {Exploration of Multi-modal Translation Resource Processing Modes under Computer Graphics: A Case Study of Visionbank},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728828},
abstract = {The rapid development of computer technology has brought computer graphics a qualitative leap in graphic images and visual communication. Computer graphics transforms digital information into image information while conducting digital processing and visualization of graphic images. Combining with the development process of computer graphics, this paper considers the application field of computer graphics, discusses the connotation deduction of multi-modal translation resource. Through case analysis of the Visionbank, this paper validates and advances computer graphics technology to optimize multi-modal translation resource, and realizes the comprehensive utilization of multi-modal translation resource.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {654–658},
numpages = {5}
}

@inproceedings{10.1145/3728725.3728818,
author = {Zhu, Qifei and Tang, Jiangang and Cao, Haoshun},
title = {An Internet Public Opinion Monitoring Model Based on Stacked Ensemble Learning},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728818},
doi = {10.1145/3728725.3728818},
abstract = {With the rapid development of social media, internet public opinion has a profound impact on social stability and economic development. At present, common monitoring models generally have problems such as insufficient accuracy or weak generalization ability. Therefore, This paper proposes an internet public opinion monitoring model based on stacked ensemble learning, which aims to realize the real-time monitoring. The model selects combines random forest (RF), logistic regression (LR), support vector machine (SVM) group as the base learner. And integrates the prediction results of the base-learners through the meta learners (GBM) to monitor public opinion in real time. The experimental results show that the empirical model achieves 0.940 accuracy and an F1 score of 0.896, which significantly outperforms other ensemble methods. It can effectively monitor the scale and trend of public opinion of internet comments, and provide strong support for decision makers.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {591–597},
numpages = {7},
keywords = {Empirical Research, Internet Public Opinion Monitoring, Stacked Ensemble Learning},
location = {
},
series = {GAIIS '25}
}

@inproceedings{10.1145/3734921.3734932,
author = {Ren, Zhiyang and Jin, Guangri},
title = {Artificial neural network-based model for predicting compressive strength of cemented gravels},
year = {2025},
isbn = {9798400713460},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3734921.3734932},
doi = {10.1145/3734921.3734932},
abstract = {In order to study the compressive strength of cemented gravel, a compressive strength prediction model was established; with 142 groups of in-house uniaxial compressive strength test data of cemented gravel with different mixing ratios as the object of study, the 11 influencing factors in the samples were downscaled by using the principal component analysis method, and the downscaled principal components were used as the input layer of the BPNN, so as to establish the compressive strength prediction of cemented gravel based on the PCA-BPNN model. The results show that when the number of principal components is 3, the cumulative contribution rate of the principal components reaches 86.483\%, and the contribution rates of the three principal components are 65.084\%, 11.702\%, 9.097\%, and the eigenvalues of the three principal components are all greater than 1. Then the three principal components are used as the input layer to build the compressive strength model of cemented gravel based on PCA-BPNN, and the RMSEs of the training set and test set are 0.253 and 0.28, respectively. The RMSE of the training set and the test set are 0.253 and 0.286, and the R2 is 0.947 and 0.942, which are not much different from the BPNN model with the number of nodes in the input layer of 6 and 11 before dimensionality reduction, and they are obviously better than the BPNN model in the convergence speed and training time. It can be seen that the PCA-BPNN model has good prediction effect and prediction efficiency, especially drastically reducing the training time, which provides a new idea for the prediction research of the mechanical properties of cemented gravel.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence and Digital Media},
pages = {63–70},
numpages = {8},
keywords = {Artificial neural network, Cemented Gravels, Compressive Strength, PCA},
location = {
},
series = {GADM '25}
}

@inproceedings{10.1145/3665348.3665390,
author = {Zheng, Yifan and Liu, B.},
title = {A secure multi-quantum voting protocol based on GHZ state},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665390},
doi = {10.1145/3665348.3665390},
abstract = {Private Set Intersection Cardinality (PSI - CA) is a cryptographic primitive through which two or more participants can obtain the intersection cardinality of their private sets and the privacy of the sets is protected. In this paper, we propose a tripartite protocol that utilizes quantum resources to accomplish these tasks, in which each two or more parties can obtain the intersection cardinality of their private sets with the help of semi-honest third parties (TP). Using this property, a voting protocol is designed. In our protocol, the GHZ state is used to encode private information that TP will use to compute the cardinality. We prove that the proposed protocol is secure against well-known quantum attacks. Furthermore, compared with existing quantum voting schemes, the protocol achieves higher efficiency.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {243–248},
numpages = {6},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inbook{10.1145/3728725.3728780,
author = {Lin, Zhenyang},
title = {Metal Level Measurement and Compensation Based on the MiDas Model},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728780},
abstract = {Accurate measurement of liquid metal levels is critical to the safety and efficiency of industrial process monitoring and automation. Traditional contact-based measurement methods face significant limitations in high temperature and extreme environments, such as safety risks, inaccurate measurements due to interference with the environment, and short equipment life. To address these challenges, this study proposes an innovative non-contact measurement method using a visual depth estimation technique based on deep learning. The MiDaS model captures an image of a liquid metal surface by a monocular camera in an industrial environment. A deep neural network then estimates their depth by processing these images. By calculating the vertical distance between the liquid metal level point and a predefined reference point, real-time and accurate monitoring of the liquid level height is realized. This study not only introduces a new non-contact solution for measuring the height of liquid metal surfaces, but also enhances the potential applications of deep learning in industrial vision measurement and depth estimation.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {342–346},
numpages = {5}
}

@inproceedings{10.1145/3715275.3732120,
author = {Pendse, Sachin R. and Gergle, Darren and Kornfield, Rachel and Meyerhoff, Jonah and Mohr, David and Suh, Jina and Wescott, Annie and Williams, Casey and Schleider, Jessica},
title = {When Testing AI Tests Us: Safeguarding Mental Health on the Digital Frontlines},
year = {2025},
isbn = {9798400714825},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3715275.3732120},
doi = {10.1145/3715275.3732120},
abstract = {The systematic testing of generative artificial intelligence (AI) models by collaborative teams and distributed individuals, often called red-teaming, is a core part of the infrastructure that ensures that AI models do not produce harmful content. Unlike past technologies, the black box nature of generative AI systems necessitates a uniquely interactional mode of testing, one in which individuals on red teams actively interact with the system, leveraging natural language to simulate malicious actors and solicit harmful outputs. This interactional labor done by red teams can result in mental health harms that are uniquely tied to the adversarial engagement strategies necessary to effectively red team. The importance of ensuring that generative AI models do not propagate societal or individual harm is widely recognized—one less visible foundation of end-to-end AI safety is also the protection of the mental health and wellbeing of those who work to keep model outputs safe. In this paper, we argue that the unmet mental health needs of AI red-teamers are a critical workplace safety concern. Through analyzing the unique mental health impacts associated with the labor done by red teams, we propose potential individual and organizational strategies that could be used to meet these needs, and safeguard the mental health of red-teamers. We develop our proposed strategies through drawing parallels between common red-teaming practices and interactional labor common to other professions (including actors, mental health professionals, conflict photographers, and content moderators), describing how individuals and organizations within these professional spaces safeguard their mental health given similar psychological demands. Drawing on these protective practices, we describe how safeguards could be adapted for the distinct mental health challenges experienced by red teaming organizations as they mitigate emerging technological risks on the new digital frontlines. Note: This work includes descriptions of violence, trauma, and mental illness.},
booktitle = {Proceedings of the 2025 ACM Conference on Fairness, Accountability, and Transparency},
pages = {1793–1804},
numpages = {12},
keywords = {generative artificial intelligence, red-teaming, mental health, AI safety},
location = {
},
series = {FAccT '25}
}

@inbook{10.1145/3728725.3728820,
author = {Chen, Xunlin and Li, Xiaoju and Chen, Ziyong},
title = {Research on Weed Identification Based on Deep Learning},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728820},
abstract = {In the intricate environment where crops and weeds grow intermingled, an effective solution to this problem is to precisely spray herbicides on weeds while accurately avoiding crops. The technical reliance in this process is the focus of this paper, which involves classifying, identifying, and locating weeds and crops. Currently, deep learning is widely used for image recognition and classification. However, the complex characteristics of the intermingled growth of weeds and crops are the main factors affecting the extraction of weed features. Based on an extensive review of domestic and foreign literature, and combining the research progress and methods of deep learning in the field of image recognition, this paper designs a method that can identify weeds with complex characteristics, effectively improving the accuracy of weed image recognition. This provides a favorable reference for the rapid and precise targeted spraying of herbicides in agricultural intelligent equipment or other devices beneficial to human life.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {605–609},
numpages = {5}
}

@inbook{10.1145/3728725.3728747,
author = {Li, Fengdi},
title = {MH-PEFT: Mitigating Hallucinations in Large Vision-Language Models through the PEFT Method},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728747},
abstract = {Large vision-language models (LVLMs) have achieved significant progress and been widely applied across various industries. However, a series of significant challenges have also been posed, such as the hallucination, where the generated attributes of a target are inconsistent with the information in the image. Recent works have primarily focused on improving generation performance by training the multimodal alignment module of the model with additional data. However, this approach incurs higher costs and introduces redundant information due to the reliance on large amounts of additional data. To address these challenges, we propose a novel multimodal mitigation framework, termed MH-PEFT. Specifically, we introduce an effective data augmentation strategy that leverages existing data to generate additional attribute-related information, thereby enhancing the model's generative capabilities. Furthermore, we develop a PEFT-based training approach specifically tailored to mitigate multimodal hallucination in LVLMs. Notably, our framework features a plug-and-play design, which enhances its robustness and facilitates its deployment in real-world scenarios. Extensive experiments demonstrate that our method achieves state-of-the-art performance across multiple benchmark datasets.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {137–142},
numpages = {6}
}

@inbook{10.1145/3728725.3728799,
author = {Wen, Chengjiang and Yue, Yang and Wang, Zhixiang},
title = {The Application of Membership Inference in Privacy Auditing of Large Language Models Based on Fine-Tuning Method},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728799},
abstract = {Thanks to the large datasets collected from the internet, large language models (LLMs) have made significant progress across various fields, with widespread applications in digital employees, intelligent customer service, and smart decision-making. However, during the training process, models may memorize sensitive private data, posing risks to data security and privacy protection. Member Inference Attacks (MIA) aim to determine whether a specific record was used in the training of the target model. This approach is crucial for detecting privacy risks in models and has seen substantial progress in both traditional and fine-tuned models. However, this remains an unresolved issue for large language models (LLMs). Recent studies indicate that under proper experimental setups, the AUC-ROC of existing methods approaches 0.5 (random guessing). Subsequently, some research has shifted MIA to dataset inference, achieving promising results in this area. Our study shows that when fine-tuning a target model with aggregated samples, it is possible to distinguish whether these samples were used in training the target model by observing the initial loss returned by the model. Based on this observation, we propose a fine-tuning-based member inference method for large language models. To validate the effectiveness of our method, we conducted extensive experiments on three mainstream models and seven datasets. The results indicate that member inference attacks with aggregated samples can achieve acceptable outcomes.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {473–479},
numpages = {7}
}

@inproceedings{10.1145/3663433.3663435,
author = {Behboudi, Atefeh and Adefisayo, Adenike Omolara and Arastoopour Irgens, Golnaz},
title = {Promoting Critical Consciousness in Students through Artifacts Co-creation with GAI},
year = {2024},
isbn = {9798400717222},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3663433.3663435},
doi = {10.1145/3663433.3663435},
abstract = {The integration of digital technologies in education offers opportunities to foster critical thinking among students. However, their application in fostering critical consciousness remains largely unexplored. This work-in-progress proposes a study to explore the role of Generative Artificial Intelligence (GAI) in promoting critical consciousness, which is the ability to recognize and take action against inequality and injustice, among middle school students.&nbsp; Framed by Constructionism theory, this study will explore designed activities where students will collaboratively use GAI to create meaningful artifacts addressing social issues. Through a curriculum embedded with GAI-driven activities, students will engage in dialogues, collaborative projects, and reflective practices on environmental injustice. The collaboration with GAI will result in the co-creation of artifacts. Qualitative methods centering on reflective interviews will shed light on students’ collaborative co-creative processes to understand how these experiences influence their perspectives and critical thinking abilities.},
booktitle = {Proceedings of the 2024 Symposium on Learning, Design and Technology},
pages = {26–29},
numpages = {4},
keywords = {Co-creation, Critical consciousness, Generative Artificial Intelligence, Technology appropriation},
location = {Delft, Netherlands},
series = {LDT '24}
}

@inproceedings{10.1145/3665463.3678856,
author = {Tucek, Tom},
