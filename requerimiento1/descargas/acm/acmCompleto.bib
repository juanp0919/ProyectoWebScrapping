@inproceedings{10.1145/3719160.3737634,
author = {Snyder, Eugene Cho},
title = {Voice Profiles vs. Text Transcripts, Do Data Modality Layers in Voice Recordings Matter to Alexa Users? The Role of Modality in Voice Data Privacy},
