publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3746469.3746595},
doi = {10.1145/3746469.3746595},
abstract = {This paper addresses the challenges faced by students during the transition to university, particularly in sports education, where students’ expectations often do not align with course designs. Students typically enter university with unrealistic academic expectations and lack necessary self-regulation skills, which negatively affect their academic success. Traditional methods of evaluating sports courses, such as expert-designed questionnaires, oversimplify students’ experiences and fail to capture the complexity of their feedback. This paper proposes a new algorithm based on sentiment analysis, utilizing natural language processing (NLP) and aspect-based sentiment analysis (ABSA) to better understand students’ multidimensional evaluations of sports courses. By employing transfer learning techniques, such as fine-tuning pre-trained large language models (e.g., Qwen-2.5 3B), this method provides a more refined and detailed analysis of student feedback. The proposed system not only quantifies students’ emotional feedback on various aspects of the course but also reflects on how these insights can guide course improvements. This paper emphasizes the importance of accurately capturing student experiences to improve course content, teaching methods, and overall student satisfaction, offering valuable contributions to the field of sports management.},
booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area Education Digitalization and Computer Science International Conference},
pages = {807–813},
numpages = {7},
keywords = {Large Language Model, Online Sports Courses, Course Reviews, Sentiment Analysis},
location = {
},
series = {EDCS '25}
}

@inproceedings{10.1145/3722237.3722292,
