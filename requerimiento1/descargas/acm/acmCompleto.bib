articleno = {9},
numpages = {10},
keywords = {Augmented Reality (AR), Deep Learning, Digital Museum, Gamification, Generative Artificial Intelligence, Photogrammetry, Virtual Museum, Virtual Reality (VR)},
location = {Guimar\~{a}es, Portugal},
series = {Web3D '24}
}

