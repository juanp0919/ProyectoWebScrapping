publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3766918.3766974},
doi = {10.1145/3766918.3766974},
abstract = {This paper presents a comparative evaluation of three topic modeling approaches—Latent Dirichlet Allocation (LDA), BERTopic, and TF-IDF combined with KMeans clustering—for analyzing news articles. Using the 2025-updated BBC News dataset comprising ≈ 39, 653 short-text descriptions, we assess the models based on key metrics including CV coherence, topic diversity, silhouette score, and computational efficiency. The experimental workflow involves data preprocessing, model implementation, visualization (word clouds, topic distributions, and similarity heatmaps), and quantitative evaluation. Results indicate that BERTopic excels in semantic coherence and topic diversity due to its transformer-based embeddings, making it ideal for interpretability in news analysis, while TF-IDF + KMeans offers superior speed for large-scale applications, and LDA provides a balanced probabilistic baseline. An ablation study on BERTopic components further validates the contributions of UMAP, HDBSCAN, and c-TF-IDF. These findings highlight trade-offs in model selection for news text mining and suggest directions for hybrid approaches in future work.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence for Business},
pages = {334–337},
numpages = {4},
keywords = {topic modeling, Latent Dirichlet Allocation (LDA), BERTopic, TF-IDF, KMeans, news analysis, text mining, natural language processing, coherence, topic diversity},
location = {
},
series = {GAIB '25}
}

