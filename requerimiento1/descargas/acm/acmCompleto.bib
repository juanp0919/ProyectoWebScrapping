@inproceedings{10.1145/3766918.3766944,
author = {Huang, Rui and Zhao, Zimo and Chen, Siwen and Wu, Xiaoyu and Zhao, J. Leon},
title = {Wealth-Voyager: Navigating IntelligentWealth Management with a Multi-Agent Framework},
year = {2025},
isbn = {9798400716027},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3766918.3766944},
doi = {10.1145/3766918.3766944},
abstract = {Rising demand for hyper-personalized wealth management is increasingly unmet by traditional advisory models, which suffer from limitations in scalability, cost-efficiency, and objectivity. While recent AI-driven solutions show promise, they often remain fragmented, focusing on isolated tasks and lacking a unified architecture that integrates long-term strategy with dynamic behavioral adaptation. To bridge this gap, we introduce Wealth-Voyager, a multi-agent framework that synergizes strategic asset allocation with adaptive tactical adjustments. Our system’s architecture uniquely combines AlphaForge, a quantitative engine for establishing a long-term strategic portfolio, with DualAdvisor, a novel Belief-Desire-Intention (BDI) grounded role-playing simulation that interactively manages user-specific behavioral biases in response to real-time events. Our proof-of-concept case study, conducted under live market conditions, demonstrated the efficacy of this integrated approach. The system’s adaptive tactical adjustments outperformed a passive baseline, converting a marginal gain into a more substantial return while reducing portfolio volatility. Our work presents a blueprint for a new class of financial co-pilots that integrate quantitative rigor with interactive, cognitively-aware guidance, enhancing both decision quality and user trust.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence for Business},
pages = {156–165},
numpages = {10},
keywords = {Generative large language models (LLMs), Multi-agent systems, Behavioral finance, Portfolio optimization, Personalized advisory},
location = {
},
series = {GAIB '25}
}

@inproceedings{10.1145/3766918.3766940,
author = {Jiang, Jialin and Liu, Wangping and Xiao, Shengyao},
title = {Explainable AI-Driven Analysis of Digital Finance on Rural Industrial Transformation},
year = {2025},
isbn = {9798400716027},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3766918.3766940},
doi = {10.1145/3766918.3766940},
abstract = {This research investigates how digital finance (DIF) accelerates rural industrial transformation (IS) across China using advanced machine learning techniques. The study analyze provincial panel data through interpretable Gradient Boosting Regression Trees (GBRT) integrated with SHAP values and ALE plots. Results demonstrate that DIF significantly promotes IS while revealing heterogeneous impacts from agricultural economic scale (AES), urbanization level (URB) and regional economic development level (lnGDP). Robustness tests confirm digital finance's positive effect persists under subdimensional replacements and lagged specifications. Critically, AES exerts counterproductive pressure whereas URB and lnGDP enhance restructuring. This research pioneers an explainable AI framework for industrial economics proving digital finance strategically deployed accounts for regional disparities can drive sustainable rural modernization.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence for Business},
pages = {127–132},
numpages = {6},
keywords = {Digital Finance, Explainable AI Techniques, GBRT, Rural Industrial Transformation},
location = {
