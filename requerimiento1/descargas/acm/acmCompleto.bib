isbn = {9798400716027},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3766918.3766963},
doi = {10.1145/3766918.3766963},
abstract = {In generative image models, the quality of textual prompts directly influences the accuracy and coherence of the generated outputs. This paper focuses on how refined textual prompt design can enhance the precision of image generation. It systematically analyzes the linguistic strategies used in subject description, environment construction, style constraints, lighting control, and compositional elements. By comparing the outcomes of vague prompts with those of detailed prompts, the study demonstrates that clear attribute specifications (such as age, ethnicity, and emotional state), spatiotemporal scene details (e.g., weather conditions, background dynamics), and explicit boundary settings (e.g., negation constraints, spatial symmetry) are critical to reducing image distortion and enhancing semantic consistency. Furthermore, the paper proposes a multi-element prompt template to serve as a practical linguistic control framework for image generation tasks. The study emphasizes that prompts are not merely input commands but essential logical units for output regulation, and their refinement is vital for improving both the quality and controllability of AI-generated images.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence for Business},
pages = {266–271},
numpages = {6},
keywords = {Fine-grained Control, Generative Image Models, Prompt Engineering, Semantic Consistency, Textual Prompts},
location = {
