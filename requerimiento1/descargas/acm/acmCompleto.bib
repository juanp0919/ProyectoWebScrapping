series = {GAIB '25}
}

@inproceedings{10.1145/3766918.3766935,
author = {Guan, Shuxin and Wang, Zihan and Zhao, Shengyu},
title = {An Empirical Study on the Carbon Emission Reduction Effect of Green Bond Issuance},
year = {2025},
isbn = {9798400716027},
publisher = {Association for Computing Machinery},
