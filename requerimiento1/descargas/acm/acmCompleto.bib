@inproceedings{10.1145/3673805.3673828,
author = {S\"{o}derstr\"{o}m, Ulrik and Hedstr\"{o}m, Elsa and Lambertsson, Karl and Mejtoft, Thomas},
title = {ChatGPT in education: Teachers’ and Students’ views},
year = {2024},
isbn = {9798400718243},
