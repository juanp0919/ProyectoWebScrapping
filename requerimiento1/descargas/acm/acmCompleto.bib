year = {2025},
isbn = {9798400720369},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3746058.3759017},
doi = {10.1145/3746058.3759017},
abstract = {Virtual reality is increasingly used in education, gaming, and creative fields, yet 3D modeling remains a complex process, limiting natural and efficient interaction. While generative AI has advanced text-to-3D techniques, intuitive inputs like hand gestures are still underexplored. Inspired by everyday gesture-based expression, this paper proposes a method that combines bimanual gestures with voice input to rapidly generate graspable 3D props in VR. By analyzing the spatial features and implicit intent of hand gestures, the system automatically infers the object’s size, grip type, and interaction pose, enabling natural and well-fitted 3D model.},
booktitle = {Adjunct Proceedings of the 38th Annual ACM Symposium on User Interface Software and Technology},
articleno = {38},
numpages = {4},
keywords = {Virtual Reality, 3D Content Generation, Hand Gesture Interaction},
