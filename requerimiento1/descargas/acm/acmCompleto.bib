@inproceedings{10.1145/3706599.3719281,
author = {Heo, Sumin and Chen, Erika R and Khuu, Jasmine},
title = {Exploring Gender Biases in LLM-based Voice Chatbots for Job Interviews},
year = {2025},
isbn = {9798400713958},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3706599.3719281},
doi = {10.1145/3706599.3719281},
abstract = {The use of artificial intelligence (AI) and voice user interfaces (VUIs) in hiring processes is a rapidly growing trend. Since the COVID-19 pandemic, an increasing number of companies have adopted automated systems for conducting job interviews. In traditional interview settings, research has shown that gendered interactions between the interviewer and the interviewee can influence performance and hiring decisions. Addressing this issue within the digital job-seeking context, our study investigates whether similar effects occur in interactions with LLM-based voice chatbots during mock job interviews. We conducted a study using two gendered AI interviewer conditions: a feminine AI voice and a masculine AI voice. The study examined how the AI interviewer’s gender presentation influenced interviewees’ lexical diversity, willingness to disclose personal information, and confidence levels. Preliminary findings indicate that gender stereotypes persist in interactions with AI chatbots, with participants associating stereotypical feminine traits with the feminine AI voice. Furthermore, participants reported a more positive overall interview experience with the feminine voice compared to the masculine voice.},
booktitle = {Proceedings of the Extended Abstracts of the CHI Conference on Human Factors in Computing Systems},
articleno = {899},
numpages = {8},
keywords = {Voice User Interfaces, Human-AI Interaction, AI Gender Representation, Job interviews},
location = {
},
series = {CHI EA '25}
}

@inproceedings{10.1145/3711403.3711442,
author = {Zhang, Simo and Hsiao, Ming-Yu},
title = {Research on the Construction of AI-Assisted Art Installation Design Translation Models and Their Application in Design Education},
year = {2025},
isbn = {9798400717468},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3711403.3711442},
doi = {10.1145/3711403.3711442},
abstract = {In December 2022, Midjourney's public beta test attracted significant attention, especially in 2023, due to its capability to generate comic and illustration styles, highlighting the pivotal role of generative AI images in the future development of technology and art design. Currently, generative AI images are increasingly being utilized as auxiliary tools in fields such as product design, illustration, and sculpture, enhancing the efficiency of design proposals. In design practice education, generative AI images are being integrated into classroom settings, thereby becoming progressively involved in the study of design theory. The use of generative AI images in design education is emerging as a trend in future educational development. As cultural and aesthetic economies evolve, urban and commercial developments are increasingly relying on the integration of art and economics. Public art injects new cultural vitality and exists in various forms, such as art installations. However, there is a lack of research on the use of generative AI images in art installation design education, and teaching methods for art installation design translation are insufficient. This study employs case analysis and interviews based on the continuous interpretive nature of Peirce's Semiotics. The art installation case chosen for this study is "Taichung Hybrid" from the "Touch Taichung" outdoor art installation event. The study explores teaching research in art installation design translation, incorporating generative AI images. A model for AI-assisted art installation design translation is constructed and organized into teaching modes and methods.},
booktitle = {Proceedings of the 2024 7th International Conference on Educational Technology Management},
pages = {226–233},
numpages = {8},
keywords = {Art installation, Design Semiotics, Design translation, Generative AI images, Teaching methods},
location = {
},
series = {ICETM '24}
}

@inproceedings{10.1109/ASE56229.2023.00204,
author = {Pathania, Priyavanshi and Mehra, Rohit and Sharma, Vibhu Saujanya and Kaulgud, Vikrant and Podder, Sanjay and Burden, Adam P.},
title = {Towards a Knowledge Base of Common Sustainability Weaknesses in Green Software Development},
year = {2024},
isbn = {9798350329964},
publisher = {IEEE Press},
url = {https://doi.org/10.1109/ASE56229.2023.00204},
doi = {10.1109/ASE56229.2023.00204},
abstract = {With the climate crisis looming, engineering sustainable software systems become crucial to optimize resource utilization, minimize environmental impact, and foster a greener, more resilient digital ecosystem. For developers, getting access to automated tools that analyze code and suggest sustainability-related optimizations becomes extremely important from a learning and implementation perspective. However, there is currently a dearth of such tools due to the lack of standardized knowledge, which serves as the foundation of these tools. In this paper, we motivate the need for the development of a standard knowledge base of commonly occurring sustainability weaknesses in code, and propose an initial way of doing that. Furthermore, through preliminary experiments, we demonstrate why existing knowledge regarding software weaknesses cannot be re-tagged "as is" to sustainability without significant due diligence, thereby urging further explorations in this ecologically significant domain.},
booktitle = {Proceedings of the 38th IEEE/ACM International Conference on Automated Software Engineering},
pages = {1718–1720},
numpages = {3},
location = {Echternach, Luxembourg},
series = {ASE '23}
}

