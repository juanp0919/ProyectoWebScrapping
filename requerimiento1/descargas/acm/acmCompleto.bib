@inproceedings{10.1145/3766918.3766936,
author = {Ren, Runtao and Ma, Jian},
title = {Intelligent Decision Support System Design for Research Assessment Exercise},
year = {2025},
isbn = {9798400716027},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3766918.3766936},
doi = {10.1145/3766918.3766936},
abstract = {This paper proposes a framework of Generative Artificial Intelligence (GenAI)-based Decision Support System (DSS) named MetricAI tailored to the Hong Kong Research Assessment Exercise (RAE). The designed conceptual framework aims to automate the evaluation of researchers' publications across three core dimensions: originality, significance, and rigor by leveraging state-of-the-art GenAI technologies. Beyond providing quantitative scores and personalized feedback, the system enables researchers to make better decisions by integrating these evaluation results with additional supporting information, such as journal impact factors and altmetrics, thereby optimizing their research performance and RAE submission strategies. The conceptual system architecture consists of four layers: data ingestion, AI processing, decision support, and user interface. This paper focuses on proposing the architectural design of such a GenAI-based DSS, and through a preliminary interface design case study, we demonstrate how the GenAI-based DSS can provide data-driven insights through a human-machine collaborative approach, thereby improving evaluation efficiency. The MetricAI is designed to improve efficiency, transparency, and strategic nature of RAE preparation work.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence for Business},
pages = {101–106},
numpages = {6},
keywords = {Decision Support System, Generative Artificial Intelligence, Interface Design, Research Management},
location = {
},
series = {GAIB '25}
}

@inproceedings{10.1145/3766918.3766968,
author = {Li, Zhenhuan},
title = {Analysis of the Potential Application of GenAI in Supply Chain Optimization Technology},
year = {2025},
isbn = {9798400716027},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3766918.3766968},
doi = {10.1145/3766918.3766968},
abstract = {Traditional supply chain management mainly relies on historical data and static models such as autoregressive integrated moving average models (ARIMA). However, Enterprise Resource Planning (ERP) system still has the disadvantages, including delayed response, inaccurate long tail demand forecasting, and weak risk resistance and so on. Presently generative AI (GenAI) demonstrates the capability to enhance supply chain resilience significantly through synthetic data augmentation, multimodel analysis, and dynamic optimization technologies. For example, Generative Adversarial Networks (GAN) can generate extreme market scenario data to compensate for the lack of historical data and Large Language Model (LLM) can analyze social media public opinion and perceive changes in demand in advance.In this paper, the predictive ability of GenAI for future markets and the possibility of developing supply chain plans was investigated and then the market trends in 2025 and beyond were predicted based on the market data of the latest five years before 2025, using GenAI deep learning and integrating external variables such as economic indicators and simulating multiple future scenarios. The conclusions reveal that one of the most important advantages lies in AI's deep learning ability and foresight, which can identify potential supply chain disruptions in advance within a certain period of time and have adaptability, dynamically adjusting strategies to cope with sudden fluctuations. Besides, the analysis of AI application data in Lenovo, DHL, and Meituan also has been observed that GenAI has the extremely advantages to optimize time and high efficiency of production planning in supply chain management with the improved resource utilization and promoted growth of transaction order volume. Moreover, the fitting curves between the functions learned by GenAI combined with Python and traditional artificial learning models were compared, whereas, it was found that GenAI deep learning models still have shortcomings and face the challenge of data dispersion and inconsistency in supply chain optimization applications. Therefore, in the future, with the development of multimodel AI and causal reasoning, the focus of the technology should be on the research of strengthening the optimization of model algorithms, with assisting the use of mathematical methods to enhance the accuracy of GenAI models, so that this technology can truly become a core tool for dynamically predicting markets.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence for Business},
pages = {299–304},
numpages = {6},
keywords = {Generative artificial intelligence, demand forecasting, inventory management, supply chain optimization},
location = {
},
series = {GAIB '25}
}

@inproceedings{10.1145/3734921.3734944,
author = {Xu, Zhixiang and Idzwan bin Ismail, Adzrool},
title = {Generative AI-Based Dialogue Generation and Optimization in the Style of Traditional Chinese Opera Dream of the Red Chamber Using K-means Clustering},
year = {2025},
isbn = {9798400713460},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
