@article{10.1145/3769733.3769739,
author = {Trippas, Johanne R. and Culpepper, J. Shane and Aliannejadi, Mohammad and Allan, James and Amig\'{o}, Enrique and Arguello, Jaime and Azzopardi, Leif and Bailey, Peter and Callan, Jamie and Capra, Rob and Craswell, Nick and Croft, Bruce and Dalton, Jeff and Demartini, Gianluca and Dietz, Laura and Dou, Zhicheng and Eickhoff, Carsten and Ekstrand, Michael and Ferro, Nicola and Fuhr, Norbert and Glowacka, Dorota and Hasibi, Faegheh and Hettiachchi, Danula and Jones, Rosie and Kamps, Jaap and Kando, Noriko and Karimi, Sarvnaz and Kato, Makoto P. and Koopman, Bevan and Liu, Yiqun and Ma, Chenglong and Mackenzie, Joel and Maistro, Maria and Mao, Jiaxin and McKay, Dana and Mitra, Bhaskar and Mizzaro, Stefano and Moffat, Alistair and Mothe, Josiane and Ounis, Iadh and Rashidi, Lida and Ren, Yongli and Sanderson, Mark and Santos, Rodrygo and Scholer, Falk and Shah, Chirag and Sitbon, Laurianne and Soboroff, Ian and Spina, Damiano and Thomas, Paul and Urbano, Juli\'{a}n and de Vries, Arjen and White, Ryen and Yuan, Abby and Zamani, Hamed and Zendel, Oleg and Zhang, Min and Zhuang, Shengyao and Zobel, Justin and Zuccon, Guido},
