@inproceedings{10.1145/3766918.3766956,
author = {Wang, Li and Qiu, Ke and Wu, Haotian and Cheng, Hang and Chen, Tianyu},
title = {A Courses Recommendation Model Incorporating Course Information into SubGraph Convolutional Network},
year = {2025},
isbn = {9798400716027},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3766918.3766956},
doi = {10.1145/3766918.3766956},
abstract = {Online learning platforms contain rich graph data, laying a good foundation for the application of Graph Neural Network technology. As online learning platforms grow rapidly, offering precise course recommendations has gradually become crucial for their sustainable development. However, current recommendation models often suffer from insufficient mining of course's information and inadequate capture of higher-order connectivity information. Therefore, we propose a Courses Recommendation Model Incorporating Course Information into SubGraph Convolutional Network (IISGCN-CR). The model utilizes Convolutional Neural Networks to extract the textual information of the course for the initial embedding representation of the course. Then the learner subgraphs and course subgraphs are constructed based on the similarity of learner's interests and the similarity of courses and perform graph convolution on the subgraphs to learn high-order information about learners and courses. Subsequently, the embedding representations obtained in the graph convolution are weighted and aggregated using the attention mechanism to obtain the final vector representations of learners and courses. Finally, the prediction scores are obtained by the operation of the vector inner product and the recommendation of courses is made based on the prediction results. Experiments are conducted on the MOOCCube and MovieLens-1M datasets and the model improves the recommendation results compared to the better-performing baseline method.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence for Business},
pages = {232–237},
numpages = {6},
keywords = {Attention Mechanism, Course Information, Course Recommendation, Learner's Interests, SubGraph Convolutional Network},
location = {
},
series = {GAIB '25}
}

@inproceedings{10.1145/3766918.3766989,
author = {Li, Xinyang},
title = {Research on the Impact of Digital Infrastructure Construction on Green Development in Manufacturing},
year = {2025},
isbn = {9798400716027},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3766918.3766989},
doi = {10.1145/3766918.3766989},
abstract = {Against the backdrop of the "dual-carbon" goals and digital-real economy integration, this study explores how digital infrastructure drives manufacturing green transformation. Using 2011–2022 provincial panel data, it measures manufacturing green development via a super-efficiency SBM model (incorporating pollution/emissions) and applies a system GMM dynamic panel approach to test digital infrastructure's total effect and green innovation's mediating role. Key findings: (1) Digital infrastructure significantly boosts manufacturing green development; (2) Green innovation mediates weakly (7.5\% of total effect), indicating a "death valley" in tech transfer; (3) In economically developed regions, AI technologies (e.g., big data, IoT) are more advanced, enabling industrial enterprises to more readily achieve green transformation.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence for Business},
pages = {436–440},
numpages = {5},
keywords = {Digital Infrastructure Construction, GMM Model, Green Development in Manufacturing, Green Technological Innovation},
location = {
},
series = {GAIB '25}
}

@inproceedings{10.1145/3766918.3766961,
author = {Li, Wenyong and Sun, Qi and Huang, Caihua and Lian, Guan and Lu, Rui and Che, Guoyan},
