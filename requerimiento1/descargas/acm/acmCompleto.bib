@inproceedings{10.1145/3766918.3766927,
author = {Yang, Tongyu},
title = {Assessment of the Industrial Chain Resilience under Complex Network Disruption Simulation: A Case Study of the Chinese Photovoltaic Field},
year = {2025},
isbn = {9798400716027},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3766918.3766927},
doi = {10.1145/3766918.3766927},
abstract = {This study proposes a direct, dynamic approach to evaluating the resilience of industrial chains, using complex network disruption simulations. The Chinese photovoltaic industry serves as a case study. First, we mapped the industrial chain and gathered data on production enterprises. Next, we constructed the overall industrial network and its upstream, midstream, and downstream sub-networks, followed by a preliminary analysis of the network's features. Finally, we simulated network disruptions to assess and analyze the resilience of the industrial chain. The results are as follows: (1) In the Chinese photovoltaic industry, the network density and average degree of each sub-network are generally high, indicating that enterprises are tightly connected, with strong agglomeration and transmission properties. (2) Overall, the Chinese photovoltaic industrial chain demonstrates strong resilience, with the downstream sub-network showing the highest resilience, followed by the midstream, and the upstream exhibiting the lowest resilience.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence for Business},
pages = {54–61},
numpages = {8},
keywords = {Complex Network, Disruption Simulation, Industrial Chain Resilience, Photovoltaic Field},
location = {
},
series = {GAIB '25}
}

@inproceedings{10.1145/3766918.3766957,
author = {Bai, Yingchen and Zhong, Ling},
title = {Abdominal Multi-organ Segmentation Algorithm Based on Small Samples and TransU-Net},
year = {2025},
isbn = {9798400716027},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3766918.3766957},
doi = {10.1145/3766918.3766957},
abstract = {Automated medical image segmentation technology assists doctors in making comprehensive judgments by combining multiple sources of information, enabling more accurate and detailed diagnoses for patients' conditions, the proposition of reasonable treatment plans, and reasonable disease prognosis. Such advancements significantly improve treatment outcomes, reduce surgical risks, and lower the incidence of postoperative complications. To address the poor performance of TransU-Net on small datasets and the issue of the loss of local information, we introduce a hierarchical feature extraction structure similar to the nnUNet [2] baseline network so that the algorithm can adapt to small sample datasets without compromising TransU-Net's capability for large sample operations or its transferable potential. Based on TransU-Net [1], this experiment proposes an algorithm designed for abdominal multi-organ segmentation on small sample training datasets. Experimental results demonstrate that the algorithm achieves a higher Intersection-over-Union (IoU) and a lower loss value while maintaining a nearly constant quantity of parameters. Compared with the NestedUNet model, which excels in small sample operations, the proposed model improves IoU by 2\%. Compared to the lightweight TransU-Net model, it increases IoU by 10\% and reduces the loss value by 5\%.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence for Business},
pages = {238–242},
numpages = {5},
keywords = {TransU-Net, image segmentation, medical image segmentation, small sample},
location = {
},
series = {GAIB '25}
}

@inproceedings{10.1145/3766918.3766931,
author = {Cheng, Sijia},
title = {Research on the Application of Variational Autoencoders in Audit Risk Assessment and Visualization},
year = {2025},
isbn = {9798400716027},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3766918.3766931},
doi = {10.1145/3766918.3766931},
abstract = {With the increase in the volume of data within auditing environment, the risk assessment of audit increasingly depends on data-based anomaly detection and internal control issues detection. In this paper, we propose a novel deep generative model with variational autoencoder Hierarchical Attention-Driven VAE (HAD-VAE) for audit risk scoring and review visualization. The model extends the typical VAE framework, and forms a two-layer hidden structure of latent variables for current study to capture both global behavioral patterns of the audit object and small local differences in transactions’ features. Through the design of attention mechanism and weighting to the modeling of temporal audit features, the model improves the raw skills of perceiving high risk behaviors, and leverages prior risk information from the auditing field to regularize the latent space of embedding, which enables the model to learn more discriminative risk representations. In the decoder side, the model uses two channels to reconstruct path: one channel for numerical fields and another for the semantic descriptions of transactions, which enable fine-grained modeling of the complex data, and supports the following visualization analysis. The loss function of the overall training maximizes the evidence lower bound (ELBO) and adds a contrastive loss to further separate high-risk samples from low-risk samples in the latent space. HAD-VAE achieves better performance than state-of-the-art baselines in terms of both risk scoring accuracy and anomaly localization.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence for Business},
pages = {73–77},
numpages = {5},
keywords = {Attention Mechanism, Audit Risk Assessment, Latent Variable Modeling, Variational Autoencoder},
location = {
},
series = {GAIB '25}
}

@inproceedings{10.1145/3766918.3766975,
author = {Wang, Yu and Jia, Yanrui and Guo, Fangwei},
title = {Research on the Application of Artificial Intelligence in Advertising Creativity in the Context of Metaverse Based on XGBoost},
year = {2025},
isbn = {9798400716027},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3766918.3766975},
doi = {10.1145/3766918.3766975},
abstract = {With the rapid development of information technology, the Metaverse has emerged as a new digital space. Under this perspective, advertising creativity has ushered in unprecedented changes and opportunities. The purpose of this paper is to explore how artificial intelligence (AI) empowers advertising creativity in the context of meta-universe and promotes its innovation and development. This study constructs a theoretical model with the AI participation effect as the independent variable, consumer perception as the mediator variable, and advertising expectation as the dependent variable to explore the specific influence mechanism of AI-enabled advertising creativity. Meanwhile, the XGBoost algorithm was used to construct a classification model. After data cleaning, standardization, and dividing the dataset into training and testing sets in a 7:3 ratio, parameters were optimized by combining 5-fold cross-validation. The model performance was evaluated using metrics such as accuracy and precision, and SHAP was employed to analyze feature importance. The results of the study show that the better the AI participation effect, the higher the consumer perception, and the better the advertising expectation, also consumer perception plays a mediating role between the AI participation effect and advertising expectation. On this basis, the new situation of future advertising creativity is proposed, namely, the polarization and dynamic generation of personalized ads, the paradigm change of intelligent creative production, and the intelligent placement and scenario expansion of ads.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence for Business},
pages = {338–343},
numpages = {6},
keywords = {Advertising creativity, Advertising expectation, Artificial Intelligence, Consumer perception, XGBoost},
location = {
},
series = {GAIB '25}
}

@inproceedings{10.1145/3766918.3766920,
author = {Gao, Chen},
title = {Federated P-Tuning based Time-LLM for Hotel Booking Prediction},
year = {2025},
isbn = {9798400716027},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3766918.3766920},
doi = {10.1145/3766918.3766920},
abstract = {With the rapid development of large language models (LLMs), their potential in time series prediction tasks is gradually being explored. LLMs possess strong feature representation capabilities and the ability to model complex relationships. They have shown significant advantages in areas such as text generation, classification, and time series prediction. However, pre-trained LLMs usually perform well on general tasks but are difficult to apply directly to specific tasks. Fine-tuning is required to adapt them to target domain characteristics. Meanwhile, hotel booking data is often distributed across multiple organizations. This data is privacy-sensitive and unevenly distributed, making traditional centralized training methods unsuitable for privacy protection and collaborative modeling. To address these issues, we propose FPTuning-LLM, a fine-tuning framework for large language models based on Federated Learning and P-Tuning. But time series data cannot be directly input into LLMs trained on discrete tokens. Therefore, we use Time-LLM to embed input sequences and convert them into representations suitable for LLMs. Prompt design is crucial for downstream task performance. We adopt P-Tuning to fine-tune Time-LLM, utilizing lightweight parameters to improve task adaptation. In a federated learning environment, we perform global fusion on the embedding layers of Time-LLM and the LSTM+MLP network in P-Tuning. To address the issue of non-independent and identically distributed (Non-IID) data, FedProx is used to constrain local training, enhancing model robustness and global convergence. Experimental results show that FPTuning-LLM effectively improves prediction accuracy while ensuring data privacy by leveraging private data from multiple parties.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence for Business},
pages = {7–11},
numpages = {5},
keywords = {Federated Learning, Hotel Booking Prediction, Large Language Models (LLMs), P-Tuning, Time Series Prediction},
location = {
},
series = {GAIB '25}
}

@inproceedings{10.1145/3766918.3766972,
author = {Wang, Yan and Chen, Li},
title = {Prediction and Optimization Model for Ideological and Political Education Effectiveness in Universities Based on Deep Learning and Convolutional Neural Networks},
year = {2025},
isbn = {9798400716027},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3766918.3766972},
doi = {10.1145/3766918.3766972},
abstract = {This paper proposes a deep learning-based prediction and optimization model for evaluating the effectiveness of ideological and political education (IPE) in universities. The model integrates a Multi-Scale Feature Convolutional Neural Network (MSF-CNN) with a Long Short-Term Memory (LSTM) architecture to extract and model multi-dimensional educational data, including behavioral sequences, emotional feedback, and classroom participation records. A customized optimization strategy is further embedded to convert predictive outputs into actionable teaching adjustments. Experimental evaluations on real-world datasets collected from multiple higher education institutions in Guangdong Province demonstrate that the proposed framework outperforms traditional machine learning models across all key metrics, including Accuracy, Precision, Recall, and F1-score. The system also shows substantial gains in classroom engagement, emotional stability, and academic performance after deployment. These results validate the technical viability and practical applicability of the proposed architecture in data-driven educational enhancement scenarios.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence for Business},
pages = {318–324},
numpages = {7},
keywords = {Convolutional Neural Network, Deep Learning, Educational Optimization, Ideological and Political Education, Long Short-Term Memory, Predictive Modeling, Sentiment Analysis},
location = {
},
series = {GAIB '25}
}

@inproceedings{10.1145/3766918.3766962,
author = {Chi, Han},
title = {Emotion Recognition and Enhancement for Business English Presentations Using CNN-LSTM Network},
year = {2025},
isbn = {9798400716027},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3766918.3766962},
doi = {10.1145/3766918.3766962},
abstract = {In order to solve the problems of inaccurate recognition of students' emotional states and delayed feedback in online teaching environments, this paper designs and implements a multimodal real-time emotion recognition system based on the LSTM-CNN hybrid model. The system integrates visual and audio dual-channel data, uses convolutional neural networks to extract facial image spatial features, uses long short-term memory networks to model the temporal emotional features of speech sequences, and introduces an attention mechanism in the fusion layer to improve the ability of feature collaborative expression. In the experimental part, a high-dimensional multimodal dataset covering three types of emotion labels is constructed, and cross-validation is used to evaluate the performance of the model in the emotion classification task. The experimental results show that the proposed model is superior to the baseline in terms of accuracy, robustness and cross-scenario adaptability, and the comprehensive recognition accuracy reaches 89.2\%. The study shows that this method has good promotion and practical value in educational application scenarios with complex semantics and high human-computer interaction intensity, and provides technical support for intelligent teaching feedback systems.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence for Business},
pages = {261–265},
numpages = {5},
keywords = {LSTM-CNN hybrid model, deep learning system, multimodal fusion, real-time emotion recognition, teaching human-computer interaction},
location = {
},
series = {GAIB '25}
}

@inproceedings{10.1145/3766918.3766959,
author = {You, Junqiang},
title = {Analyzing Online Consumer Sentiment with NLP and Its Implication for Brand Positioning},
year = {2025},
isbn = {9798400716027},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3766918.3766959},
doi = {10.1145/3766918.3766959},
abstract = {Online consumer sentiment plays a critical role in shaping brand perception, yet extracting meaningful insights from unstructured data remains challenging. This study proposes a transformer-based sentiment analysis framework using BERT and RoBERTa to interpret consumer opinions from Twitter, Amazon, and Yelp. The objective is to evaluate model performance in sentiment classification and demonstrate its utility in informing brand positioning strategies. Fine-tuned models achieved high classification accuracy, with RoBERTa outperforming others at up to 96.3\% accuracy on Amazon and 92\% average accuracy across datasets. Compared to traditional classifiers (fastText, SVM), transformer models demonstrated superior adaptability, especially in handling informal or context-heavy language. The findings show how sentiment trends—positive reinforcement or recurring criticism—can inform strategic brand decisions. This work highlights the potential of NLP in marketing analytics and recommends future improvements in sarcasm detection and model explainability for better business alignment.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence for Business},
pages = {248–252},
numpages = {5},
keywords = {BERT, Brand Positioning, Consumer Feedback, RoBERTa, Sentiment Analysis, Transformer Models},
location = {
