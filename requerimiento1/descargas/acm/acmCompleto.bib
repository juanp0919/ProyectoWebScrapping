@inproceedings{10.1145/3766918.3766986,
author = {Gu, Mengqi and Wang, Wencheng and Deng, Qian},
title = {Financial Statement Automatic Classification and Prediction Model Based on Convolutional Neural Networks},
year = {2025},
isbn = {9798400716027},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3766918.3766986},
doi = {10.1145/3766918.3766986},
abstract = {In the field of intelligent financial analytics, traditional rule-based and statistical methods often struggle with low efficiency and limited generalizability when handling large-scale corporate reporting data. To address these limitations, this paper proposes a lightweight convolutional neural network (CNN)-based multi-task model for the automatic classification and forecasting of financial statements. By encoding structured financial tables into matrix form and applying a custom-designed CNN architecture with shared and task-specific layers, the model jointly performs corporate health classification and key indicator prediction. The model is trained and evaluated on a dataset of 90 publicly listed companies across three major industries, utilizing real-world balance sheets, income statements, and cash flow reports. Experimental results demonstrate that the proposed method outperforms baseline models such as SVM, Random Forest, and Gradient Boosting, achieving a classification F1-score of 91.5\% and a regression R² of 0.852. Ablation studies further validate the importance of convolutional depth and multi-task learning in improving generalization and accuracy. The model offers a scalable and adaptable solution for enterprise-level financial document processing, with potential applications in automated risk assessment, credit analysis, and audit support systems.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence for Business},
pages = {417–423},
numpages = {7},
keywords = {Convolutional Neural Networks (CNN) 1, Corporate Health Prediction 7, Deep Learning 6, Financial Document Classification 5, Financial Statement Analysis 2, Multi-task Learning 3, Time-series Forecasting 4},
location = {
},
