@inbook{10.1145/3711129.3711253,
author = {Guo, Liqing and Zhang, Qin and Li, Wenyao and Yang, Qunwei},
title = {Design and Implementation of an Automatic Scoring Tool for Adaptive Screen Resolutions},
year = {2025},
isbn = {9798400710094},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3711129.3711253},
abstract = {Xiamen H University's adoption of the Future Education System V4.0 for the final assessment of "Fundamentals of Computer Science" implements paperless examinations, reducing the burden on teachers. However, the system has deficiencies in batch automatic correction and student learning status analysis, and does not adapt well to varying screen resolutions. To address these issues, this study utilizes Python to develop an automatic scoring tool for the existing system, designed to adapt to various screen resolutions. The tool employs a layered architecture to create an efficient and stable automatic scoring solution, integrating functions such as parameter setting, automatic scoring, grade analysis, and file renaming. This tool enhances the efficiency and accuracy of marking work, enables a comprehensive assessment of students' mastery of computer course content, and improves the system's user-friendliness, providing a new approach to examination and assessment methods in the education field during the digital transformation process.},
booktitle = {Proceedings of the 2024 8th International Conference on Electronic Information Technology and Computer Engineering},
pages = {722–726},
numpages = {5}
}

@inproceedings{10.1145/3627915.3628020,
author = {Sun, Huaijin and He, Ying},
title = {The Application and User Acceptance of AIGC in Network Audiovisual Field: Based on the Perspective of Social Cognitive Theory},
year = {2023},
isbn = {9798400700590},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3627915.3628020},
doi = {10.1145/3627915.3628020},
abstract = {[Purpose/Significance] Chinese network audiovisual has become the largest Internet application in China, by combing the application and user acceptance of AIGC in the field of network audiovisual, this paper discusses the standardization, rationalization and high-quality development of AIGC deeply embedded in the field of network audiovisual.[Design/Methodology] Based on the research framework of "environment - subject cognition - behavior" of social cognition theory, this paper constructs a model of "network audiovisual environment - self-determination theory-user acceptance" of AIGC, studies the application of AIGC in network audiovisual environment from three aspects of generation carrier, generation technology and generation digital life. According to the self-determination theory of psychology, this paper studies the user's autonomy, competence and relatedness, and then discusses the user's acceptance behavior.[Findings/Conclusion] By analyzing the application process of AIGC to be accepted by users in the field of network audiovisual, it is found that there are problems of data source, use ethics and copyright ownership of AIGC, and threats from data security, digital colonization, and users’ ideology.[Originality/Value] The analysis of AIGC is combined with the network audiovisual, which is the largest application environment of the Internet in China, and using communication science and social psychology to carry out cross-research, it brings new creative ideas to the analysis of AIGC and puts forward countermeasures to the potential threats of AIGC.},
booktitle = {Proceedings of the 7th International Conference on Computer Science and Application Engineering},
articleno = {11},
numpages = {5},
keywords = {Artificial Intelligence Generated Content (AIGC), Network Audiovisual Field, Self-determination Theory, Social Cognitive Theory, User Acceptance},
location = {Virtual Event, China},
series = {CSAE '23}
}

@inbook{10.1145/3730436.3730449,
author = {Shen, Yixian and Zhang, Hang and Shen, Yanxin and Wang, Lun and Shi, Chuanqi and Du, Shaoshuai and Tao, Yiyi},
title = {AltGen: AI-Driven Alt Text Generation for Enhancing EPUB Accessibility},
year = {2025},
isbn = {9798400713637},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3730436.3730449},
abstract = {Digital accessibility is essential for the equitable dissemination of content; however, certain EPUB files fail to adhere to fundamental accessibility standards, particularly in the provision of alternate text (alt text) for images. Alt text is indispensable for visually impaired individuals to understand visual content through assistive technologies. Nonetheless, the manual generation of high-quality alt text at scale continues to pose a considerable difficulty because of the requisite skill and time involved.This paper introduces AltGen, a framework utilizing generative AI to automate the development of alt text for EPUB files. AltGen uses pre-trained transformer models to provide descriptive and contextually pertinent alt text. It employs a structured process encompassing data extraction, visual content analysis with computer vision models (e.g., CLIP, ViT), and context-sensitive text generation utilizing fine-tuned large language models. The produced alt-text is assessed by computational measures, including cosine similarity and BLEU scores, and qualitative input from visually challenged individuals. Experimental findings demonstrate that AltGen lowers accessibility errors by 97.5\%, with participants noting substantial enhancements in content understanding and usability.},
booktitle = {Proceedings of the 2025 International Conference on Artificial Intelligence and Computational Intelligence},
pages = {78–83},
numpages = {6}
}

@inproceedings{10.1145/3746027.3755141,
author = {Wang, Jianhui and He, Yangfan and Zhong, Yan and Song, Xinyuan and Su, Jiayi and Feng, Yuheng and Wang, Ruoyu and He, Hongyang and Zhu, Wenyu and Yuan, Xinhang and Zhang, Miao and Li, Keqin and Chen, Jiaqi and Shi, Tianyu and Wang, Xueqian},
title = {Twin Co-Adaptive Dialogue for Progressive Image Generation},
year = {2025},
isbn = {9798400720352},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3746027.3755141},
doi = {10.1145/3746027.3755141},
abstract = {Modern text-to-image generation systems have enabled the creation of remarkably realistic and high-quality visuals, yet they often falter when handling the inherent ambiguities in user prompts. In this work, we present Twin-Co, a framework that leverages synchronized, co-adaptive dialogue to progressively refine image generation. Instead of a static generation process, Twin-Co employs a dynamic, iterative workflow where an intelligent dialogue agent continuously interacts with the user. Initially, a base image is generated from the user's prompt. Then, through a series of synchronized dialogue exchanges, the system adapts and optimizes the image according to evolving user feedback. The co-adaptive process allows the system to progressively narrow down ambiguities and better align with user intent. Experiments demonstrate that Twin-Co not only enhances user experience by reducing trial-and-error iterations but also improves the quality of the generated images, streamlining creative process across various applications.},
booktitle = {Proceedings of the 33rd ACM International Conference on Multimedia},
pages = {3645–3653},
numpages = {9},
keywords = {human-in-the-loop, interactive image generation, text-to-image generation},
location = {Dublin, Ireland},
series = {MM '25}
}

@article{10.5555/3737313.3737340,
author = {Crocetti, Giancarlo and Bak, Seonwoo and Noory, Naqib A. and Vautor-Laplaceliere, Daena D.},
title = {Evaluating the Pedagogical Impact of Large Language Models on Programming Skills in Higher Education},
year = {2025},
issue_date = {April 2025},
publisher = {Consortium for Computing Sciences in Colleges},
address = {Evansville, IN, USA},
volume = {40},
number = {8},
issn = {1937-4771},
abstract = {This empirical study investigated the impact of Generative AI (GenAI) tools, particularly large language models (LLMs), on college students' Python programming skills in a graduate-level data science course. Using a pretest-posttest methodology and accounting for variables like prior programming experience, the research examined how guided LLM usage affected students' self-assessed programming abilities. The findings revealed that while LLMs positively influenced students' capacity to develop complex applications, work with Python libraries, and write quality code, they had no significant impact on students' grasp of fundamental Python concepts or their general comfort with the language. These results suggest that LLMs serve as effective tools for advancing practical programming skills but cannot substitute for the foundational programming knowledge that must be developed through traditional learning.},
journal = {J. Comput. Sci. Coll.},
month = may,
pages = {163–177},
numpages = {15}
}

