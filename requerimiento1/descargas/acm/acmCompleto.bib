numpages = {3},
keywords = {generative AI, developer experience, well-being, productivity, cognitive load, wearables, software engineering, real-life setting, psychophysiology},
location = {Ottawa, Ontario, Canada},
series = {ICSE '25}
}

@inproceedings{10.1145/3759275.3759296,
author = {Cheng, Menghan and Lu, Zhaolin},
