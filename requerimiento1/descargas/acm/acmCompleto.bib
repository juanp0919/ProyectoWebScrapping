@inproceedings{10.1145/3745676.3745714,
author = {Wang, Zhiqiang and Huang, Zimo and Xiao, Sheng},
title = {From “Non-separation of Adjudication and Management” to Comprehensive Platform-Based Supervision and Management: Aligning the Development of Trial Management with Judicial Fairness and Efficiency},
year = {2025},
isbn = {9798400715150},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3745676.3745714},
doi = {10.1145/3745676.3745714},
abstract = {This article discusses the evolution of trial management in China——from early practices that combined judicial and administrative functions to the new stage of digital and intelligent supervision. At the beginning, the reforms focused on standardizing procedures and promoting the separation between adjudication and management. This helped reduce administrative interference in judicial work and improved both the independence of judges and the efficiency. Also, The establishment of specialized departments, such as trial management offices, has provided organization guarantees for this goal. However, as administrative control increased, it also led to some problems such as the lengthy process and the program idling. In response, recent reforms have turned to use smart court technologies to reconstruct management model. Guided by computational jurisprudence theory, courts in multiple regions have begun utilizing artificial intelligence and big data tools to supervise and assist throughout the entire litigation process. Through technological applications such as automated case handling, real-time procedural monitoring, and standardized processing of similar cases, these innovations streamline management hierarchies while enhancing the quality and efficiency of adjudication. Looking ahead, reforms need to especially pay attention to the collaborative innovation between different subjects, transparency of algorithmic systems, and improve the risk prevention and control mechanism, all of which will help giving important supply to building a more intelligent and reliable judicial system based on the principles of digital justice.},
booktitle = {Proceedings of the 2025 2nd International Conference on Innovation Management and Information System},
pages = {258–266},
numpages = {9},
keywords = {Comprehensive Platform-Based Management, Computational Law, Judicial Efficiency, Trial Management},
location = {
},
series = {ICIIS '25}
}

@inproceedings{10.1145/3613905.3651083,
author = {Shibani, Antonette and Knight, Simon and Kitto, Kirsty and Karunanayake, Ajanie and Buckingham Shum, Simon},
title = {Untangling Critical Interaction with AI in Students’ Written Assessment},
year = {2024},
isbn = {9798400703317},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3613905.3651083},
doi = {10.1145/3613905.3651083},
abstract = {Artificial Intelligence (AI) has become a ubiquitous part of society, but a key challenge exists in ensuring that humans are equipped with the required critical thinking and AI literacy skills to interact with machines effectively by understanding their capabilities and limitations. These skills are particularly important for learners to develop in the age of generative AI where AI tools can demonstrate complex knowledge and ability previously thought to be uniquely human. To activate effective human-AI partnerships in writing, this paper provides a first step toward conceptualizing the notion of critical learner interaction with AI. Using both theoretical models and empirical data, our preliminary findings suggest a general lack of Deep interaction with AI during the writing process. We believe that the outcomes can lead to better task and tool design in the future for learners to develop deep, critical thinking when interacting with AI.},
booktitle = {Extended Abstracts of the CHI Conference on Human Factors in Computing Systems},
articleno = {357},
numpages = {6},
keywords = {Artificial Intelligence, Assessment, CIAW, ChatGPT, Computing, Critical Interaction, Data Science, Education, GenAI, Writing},
location = {Honolulu, HI, USA},
series = {CHI EA '24}
}

