publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3766918.3766955},
doi = {10.1145/3766918.3766955},
abstract = {Online transaction fraud detection faces significant challenges, such as severe class imbalance, feature heterogeneity, and the necessity for real-time inference. In this study, we introduce a hybrid framework that combines multimodal feature engineering with an enhanced LightGBM model. Key contributions include a cyclic spatiotemporal encoding scheme to capture temporal fraud patterns, a regex-driven device fingerprint parser with knowledge graph-based risk profiling, and a dynamic class weighting mechanism specifically designed for evolving fraud trends. Experiments on the IEEE-CIS benchmark show that our framework achieves an AUC of 0.968—surpassing strong baselines by 12.6\%—while ensuring low inference latency (3 ms) and an exceptionally low false positive rate (0.7\%). Our results provide robust evidence for the framework's effectiveness in real-world, large-scale online fraud detection scenarios.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence for Business},
