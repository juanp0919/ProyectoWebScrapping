@article{RUSCHEMEIER2024101953,
title = {Automation bias in public administration – an interdisciplinary perspective from law and psychology},
journal = {Government Information Quarterly},
volume = {41},
number = {3},
pages = {101953},
year = {2024},
issn = {0740-624X},
doi = {https://doi.org/10.1016/j.giq.2024.101953},
url = {https://www.sciencedirect.com/science/article/pii/S0740624X24000455},
author = {Hannah Ruschemeier and Lukas J. Hondrich},
keywords = {Legal theory, Public administration, Qualitative review, Legal analysis, AI},
abstract = {The objective of this paper is to break down the widely presumed dichotomy, especially in law, between fully automated decisions and human decisions from a psychological and normative perspective. This is particularly relevant as human oversight is seen as an effective means of quality control, including in the current AI Act. The phenomenon of automation bias argues against this assumption. We have investigated this phenomenon of automation bias, as a behavioral effect of and its implications in normative institutional decision-making situations. The phenomenon of automation bias, whereby individuals overly rely on machine-generated decisions or proposals, has far-reaching implications. Excessive reliance may result in a failure to meaningfully engage with the decision at hand, resulting in an inability to detect automation failures, and an overall deterioration in decision quality, potentially up to a net-negative impact of the decision support system. As legal systems emphasize the role of human decisions in ensuring fairness and quality, this paper critically examines the inadequacies of current EU and national legal frameworks in addressing the risks of automation bias. Contributing a novel perspective, this article integrates psychological, technical, and normative elements to analyze automation bias and its legal implications. Anchoring human decisions within legal principles, it navigates the intersections between AI and human-machine interactions from a normative point of view. An exploration of the AI Act sheds light on potential avenues for improvement. In conclusion, our paper proposes four steps aimed at effectively countering the potential perils posed by automation bias. By linking psychological insights, legal analysis, and technical implications, this paper advocates a holistic approach to evolving legal frameworks in an increasingly automated world.}
}
@article{SHI2025114087,
title = {Prediction of long-term failure pressure of plastic pipe reinforced by cross helically wound steel wires},
journal = {Thin-Walled Structures},
pages = {114087},
year = {2025},
issn = {0263-8231},
doi = {https://doi.org/10.1016/j.tws.2025.114087},
url = {https://www.sciencedirect.com/science/article/pii/S0263823125011759},
author = {Jun Shi and Jie Zhou and Jing Rao and Li Zeng and Xiang Li},
keywords = {Composite pipe, time dependent property, failure pressure, load transfer},
abstract = {Plastic pipe reinforced by cross helically wound steel wires (PSP) is a new type of steel reinforced thermoplastic pipe, which is composed of steel wires and high-density polyethylene (HDPE). PSP exhibits time-dependent mechanical behavior when undertaking a long-period load due to its viscoelastic polymer matrix. Therefore, to ensure the long-term safety of PSP system, it is essential to study the long-term mechanical performance and propose a method to predict the long-term failure pressure of PSP. The work in this study is as follows. Firstly, groups of dynamic mechanical analysis (DMA) tests were conducted under different temperatures to get the creep compliance of the HDPE, owing to the interconversion method between frequency- and time-dependent material functions. Prominent nonlinear mechanical behavior of the steel wire was obtained through uniaxial tensile tests. Then long-term hydrostatic tests of PSP were carried out for validation of the following theoretical model. Secondly, a long-term theoretical analysis model of the PSP was proposed considering the elastoplastic property of the steel wire and the viscoelasticity of the HDPE. The return-mapping algorithm was integrated in the theoretical model to precisely describe the nonlinear mechanical performance of steel wire, and an iterative computation was employed to determine the viscoelastic response of the HDPE matrix in recursive forms. Afterwards, a long-term theoretical model of the PSP was established referring to equilibrium equations of a cylindrical shell subject to internal pressure, and a computational procedure was developed to implement the iterative computation process. Then, the variation of the long-term failure pressure of the PSP with time could be theoretically predicted, and the prediction results aligned well with the long-term hydrostatic test results, with small errors ranging from -3.96% to 6.35%. The causes of errors were analyzed. Furthermore, based on the model, it was studied that the influences of different factors on the mechanical performance of the PSP, including the steel-wire plastic behavior, steel wires’ number, steel wires’ radius, pipe thickness. This research might be helpful for composite structures containing reinforcements and matrix of nonlinear mechanical properties.}
}
