@article{SCHREIBER2025520,
title = {AI for cyber-security risk: harnessing AI for automatic generation of company-specific cybersecurity risk profiles},
journal = {Information and Computer Security},
volume = {33},
number = {4},
pages = {520-546},
year = {2025},
issn = {2056-4961},
