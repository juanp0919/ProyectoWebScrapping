@article{QI2025151864,
title = {Biohydrogen production from alkaline co-fermentation of blackwater and corn stover: alkali-urea combined with freeze-thaw pretreatment},
journal = {International Journal of Hydrogen Energy},
volume = {185},
pages = {151864},
year = {2025},
issn = {0360-3199},
doi = {https://doi.org/10.1016/j.ijhydene.2025.151864},
url = {https://www.sciencedirect.com/science/article/pii/S0360319925048670},
author = {Nan Qi and Wenfei Li and Hongxu Bao and Zimo Liu and Ying Xu and Jian Wang},
keywords = {Corn stover, Blackwater, Combined pretreatment, Alkaline co-fermentation},
abstract = {Direct alkaline hydrogen fermentation following alkaline pretreatment of corn stover not only enhances hydrogen production efficiency and effectively shortens the fermentation process, thereby reducing energy consumption and overall costs. The corn stover (CS) was pretreated by combined alkali-urea and freeze-thaw (AUFT) pretreatment. The optimization of pretreatment conditions and the mechanism of the AUFT pretreatment were elucidated. The AUFT pretreatment was optimized for hydrolysis under one freeze-thaw cycle, a 5 % urea concentration, and a pH of 10. Reducing sugars and SCOD concentrations reached 142.99 mg/g and 216.50 mg/g, respectively, representing 129.52 % and 172.51 % enhancements over the control. After AUFT treatment, scaly structures and holes were formed on the surface of the corn stover; the crystallinity of pretreated corn stover was significantly increased; chemical bonds between lignin and carbohydrate were cleaved, and the structure was disrupted. Co-fermentation of pretreated corn stover with black water could achieve a favorable hydrogen yield under alkaline fermentation conditions. The maximum hydrogen yield of 28.2 mL/g was obtained at pH = 10, which was 370 % higher than the control. This research provides critical data for direct fermentation after alkaline pretreatment, eliminating the need for solid-liquid separation, and offers a novel and low-cost pathway for efficient hydrogen conversion from agricultural cellulose waste in low-carbon technologies.}
}
