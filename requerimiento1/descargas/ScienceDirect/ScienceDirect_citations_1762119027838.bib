@article{TRIANA20251201,
title = {Proof-of-Concept Prompted Large Language Model for Radiology Procedure Request Routing},
journal = {Journal of Vascular and Interventional Radiology},
volume = {36},
number = {7},
pages = {1201-1207},
year = {2025},
issn = {1051-0443},
