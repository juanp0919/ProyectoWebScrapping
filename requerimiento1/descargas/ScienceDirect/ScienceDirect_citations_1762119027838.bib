title = {POS0310 CARDIAC SARCOIDOSIS IS ASSOCIATED WITH A HIGH MORTALITY AND RELAPSE RATE: RETROSPECTIVE ANALYSIS OF A COHORT AT MAYO CLINIC FLORIDA},
journal = {Annals of the Rheumatic Diseases},
