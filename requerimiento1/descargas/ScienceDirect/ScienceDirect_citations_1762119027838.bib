@incollection{PANGARSO20269,
title = {"ChatGPT Short Review"},
editor = {Vanessa Ratten},
booktitle = {International Encyclopedia of Business Management (First Edition)},
publisher = {Academic Press},
edition = {First Edition},
address = {Oxford},
pages = {9-17},
year = {2026},
isbn = {978-0-443-13702-0},
doi = {https://doi.org/10.1016/B978-0-443-13701-3.00327-3},
url = {https://www.sciencedirect.com/science/article/pii/B9780443137013003273},
author = {Astadi Pangarso},
keywords = {Bibliometric, Chatgpt, Existing knowledge, Future research, Review},
abstract = {ChatGPT is a problematic sort of AI technology now in use. As a form of technology, ChatGPT is anticipated to facilitate human-technology contact that has numerous positive effects on human existence. Nonetheless, there are still documents describing the negative aspects of chatGPT if it does not receive more focused attention and administration. In the three months following the COVID-19 epidemic, from December 2022 to March 2023, chatGPT was a trendy subject. This paper examined Scopus-indexed research articles published between the knowledge cutoff date of December 2022 and the present date of March 15, 2023. The obtained publication document data was analyzed using basic bibliometrics to provide a first overview of chatGPT-related publications. From a total of 121 publication documents, the author only gets 45 documents that can be downloaded or read to be reviewed. This study offers insights into the existing knowledge level and future research paths.}
}
@article{MADATHIL2025104381,
title = {Generative AI advertisements and Human–AI collaboration: The role of humans as gatekeepers of humanity},
journal = {Journal of Retailing and Consumer Services},
volume = {87},
pages = {104381},
year = {2025},
issn = {0969-6989},
doi = {https://doi.org/10.1016/j.jretconser.2025.104381},
url = {https://www.sciencedirect.com/science/article/pii/S0969698925001602},
author = {Johnson Clement Madathil},
keywords = {Generative AI, Human-AI collaboration, Large language models, Brand morality, Ad creativity, AI anxiety},
abstract = {Generative AI technologies are rapidly advancing and are increasingly deployed by firms for marketing purposes. However, research on generative AI in the marketing domain remains limited, particularly regarding the balance of human-AI collaboration in generative AI video advertisements. This study explores and distinguishes between two types of generative AI video ads based on the balance of human-AI collaboration: (1) those featuring human-AI collaborative characters and (2) those comprising solely AI-generated characters. Using the elaboration likelihood model as a lens, this study proposes a conceptual framework with two parallel mediators: one at the cognitive level (perceived creativity) and the other at the emotional level (AI anxiety). A multi-method study was conducted, employing diverse data sources and analytical techniques. In Study 1, we analyze a real-world dataset of generative AI video advertisements on YouTube to identify the distinct types and examine audience reactions. In Study 2, an experimental design is used to causally evaluate the effects of these advertisements on the audience. The results indicate that generative AI advertisements featuring real human-AI character collaborations generate more favorable audience attitudes, as they are perceived to be more creative and evoke lower levels of AI-induced anxiety compared to advertisements composed entirely of AI-generated characters. The study underscores the critical role of humans as gatekeepers of humanity.}
}
