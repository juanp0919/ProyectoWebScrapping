@article{AMINOV2025254,
title = {Effect of the timing of endoscopic detorsion on clinical outcomes in patients with sigmoid volvulus},
journal = {Gastrointestinal Endoscopy},
volume = {102},
number = {2},
pages = {254-263},
year = {2025},
issn = {0016-5107},
