author = {Vittorio DeFranciscis and Giovanni Amabile and Marcin Kortylewski},
keywords = {oligonucleotide, cancer therapy, immunotherapy, siRNA, antisense oligonucleotides, aptamers, delivery},
abstract = {Oligonucleotide therapeutics (ONTs) represent a rapidly evolving modality for cancer treatment, capitalizing on their ability to modulate gene expression with high specificity. With more than 20 nucleic acid-based therapies that gained regulatory approval, advances in chemical modifications, sequence optimization, and novel delivery systems have propelled ONTs from research tools to clinical realities. ONTs, including siRNAs, antisense oligonucleotides, saRNA, miRNA, aptamers, and decoys, offer promising solutions for targeting previously “undruggable” molecules, such as transcription factors, and enhancing cancer immunotherapy by overcoming tumor immune evasion. The promise of ONT application in cancer treatment is exemplified by the recent FDA approval of the first oligonucleotide-based treatment to myeloproliferative disease. At the same time, there are challenges in delivering ONTs to specific tissues, mitigating off-target effects, and improving cellular uptake and endosomal release. This review provides a comprehensive overview of ONTs in clinical trials, emerging delivery strategies, and innovative therapeutic approaches, emphasizing the role of ONTs in immunotherapy and addressing hurdles that hinder their clinical translation. By examining advances and remaining challenges, we highlight opportunities for ONTs to revolutionize oncology and enhance patient outcomes.}
}
@article{KROESE2025101205,
title = {Evaluating the automated measurement of abnormal rising and lying down behaviours in dairy cows using 3D pose estimation},
journal = {Smart Agricultural Technology},
volume = {12},
pages = {101205},
year = {2025},
issn = {2772-3755},
doi = {https://doi.org/10.1016/j.atech.2025.101205},
url = {https://www.sciencedirect.com/science/article/pii/S2772375525004368},
author = {Adrien Kroese and Niclas Högberg and Elena {Diaz Vicuna} and David Berthet and Nils Fall and Moudud Alam and Lena-Mari Tamminen},
keywords = {Animal welfare assessment, Free-stall cubicle, 3D pose estimation, Rising behaviour, Lying down behaviour, Precision livestock farming},
abstract = {The structure of cubicles can hinder cows’ movements when transitioning between postures, leading to atypical motion patterns. Assessing posture transitions relies on visual observations. This study presents a framework for complementing these assessments with kinematic measurements using 3D pose estimation. A total 809 rising and 791 lying down posture transitions were recorded over 12 cubicles by 7 synchronized cameras and processed with 3D pose estimation locating the position of the poll, withers, T13 and sacrum. First, the displacement of the keypoints was used to detect phases of the posture transitions. This detection was compared with visual observations of 200 recordings. The average mean absolute difference in detected timestamps between human and machine across all phases was 0.5 s (average σ = 0.7) and was under 0.9 s for all phases. Second, indicators were scored based on spatial use and duration, and their distribution compared to existing thresholds. We observed that 59.9 % of rising bouts and 29.1 % of lying down bouts exceeded at least one threshold. Rising delay occurred in 2.8 % of rising bouts and backwards crawling in 59.2 %. Lying down duration exceeded the threshold in 28.9 % of bouts, and rear limbs shifting duration in 8.3 %. Side lunge had a binary threshold which was not adapted to continuous sensor data. Finally, we investigated the association between indicators and found distinct dimensions for head lunge and crawling. We conclude that 3D pose is useful to score posture transition indicators, and that several indicators should be used together to capture distinct dimensions.}
}
@article{LE2025108940,
title = {Photocatalytic degradation of PFOA/PFOS over lignin-carbon–supported CuMnO₂: Material characteristics and mechanistic insights},
journal = {Journal of Water Process Engineering},
volume = {79},
pages = {108940},
year = {2025},
issn = {2214-7144},
doi = {https://doi.org/10.1016/j.jwpe.2025.108940},
url = {https://www.sciencedirect.com/science/article/pii/S2214714425020136},
author = {Phuoc-Cuong Le and Thi Thao Minh and Dinh Ngo Vu and Dinh Nhi Bui and Tan Nhat and Nang Dinh Le},
keywords = {PFOA, PFOS, Photocatalytic degradation, Lignin-derived carbon/ CuMnO, Mechanism},
abstract = {In this study, a bio-derived photocatalyst—lignin‑carbon supported CuMnO2 (LC/CuMnO2)—is reported that degrades perfluorooctanoic acid (PFOA) and perfluorooctanesulfonic acid (PFOS) by integrating adsorption on polar, micro/mesoporous carbon with redox-active CuMn sites. Material characterization (XRD, Raman, XPS, SEM/TEM, BET) revealed well-dispersed CuMnO2 nanodomains and abundant surface oxygen species on LC, while EPR and scavenger quenching identify •OH and O₂•− (with minor contributions from h+ and 1O₂) as the dominant reactive oxygen species (ROS). Under optimized conditions (pH 5, C₀ = 10 mg L−1, 0.5 g L−1 catalyst, 25 °C), LC/CuMnO2 (10 wt% oxide) achieves rapid PFAS destruction, reaching ~86 % and ~ 82 % removal within 120 min, respectively, while total organic carbon decreases to ~40–45 % of the initial value, evidencing substantial defluorination with partial mineralization. HPLC-MS unambiguously tracks stepwise chain-shortening: PFOA → PFHpA → PFHxA → PFPeA → PFBA → PFPrA → TFA → CO₂/H₂O/F−; PFOS follows dual routes of (i) progressive desulfonation to TFSA with SO₄2− release and (ii) desulfonation to PFOA before the carboxylate pathway. DFT Gibbs free-energy profiles rationalize the different initiation pathways: PFOS exhibits a lower barrier via CS bond cleavage (desulfonation), while PFOA requires higher-energy CF activation/carboxylate decarboxylation; both then converge to stepwise defluorination. The catalyst is robust over five cycles, retaining >75 % activity with Cu/Mn leaching <15/10 μg L−1 and only subtle XPS shifts, and it tolerates common background ions (1–10 mM). This lignin-enabled, earth-abundant platform provides a mechanistically grounded and scalable route for PFAS degradation in water.}
}
@article{NICE2025104164,
title = {Physicochemical characterization, ecological risk assessment and spatial distribution of microplastics in Bangladesh's largest coral reef systems},
journal = {Physics and Chemistry of the Earth, Parts A/B/C},
pages = {104164},
year = {2025},
issn = {1474-7065},
doi = {https://doi.org/10.1016/j.pce.2025.104164},
url = {https://www.sciencedirect.com/science/article/pii/S1474706525003146},
author = {Md.Simoon Nice and Tapos Kumar Chakraborty and Gehendra Kharel and Baytune Nahar Netema and Khandakar Rashedul Islam and Pragga Chowdhury and Partha Chandra Debnath and Md.Sozibur Rahman and Md Abu Rayhan and Samina Zaman and Gopal Chandra Ghosh},
keywords = {Microplastic, coral island, marine pollution, risk evaluation, human health, Bangladesh},
abstract = {Microplastic (MP) pollution has emerged as a pervasive environmental threat, yet its effects on fragile marine ecosystems remain inadequately understood. This study presents a comprehensive assessment of MP contamination in Saint (St.) Martin's Island, Bangladesh's largest coral island, revealing its occurrence, characteristics, and associated ecological and human health risks. MPs were quantified and characterized in surface water, sediment, and marine biota (fish, crustaceans, and seaweeds) using stereomicroscopy and Attenuated Total Reflectance Fourier Transform Infrared Spectroscopy (ATR-FTIR). MP characteristics were fibrous shape, size > 0.1 mm (except for seaweeds), and diverse coloration. Our results revealed significant MP contamination in all matrices: surface water (141.25 ± 35.26 MPs/L), sediment (580.83 ± 131.78 MPs/kg dry weight), and biota (28.03 ± 4.86 MPs/individual fish, 20.88 ± 5.44 MPs/individual crustacean, and 8.21 ± 2.53 MPs/individual seaweed). Risk evaluation through contamination factor (CF), Nemerow pollution index (NPI), and pollution load index (PLI) revealed pollution levels from low to very high, with polypropylene (PP) and polyethylene (PE) being predominant. Among the biota, Glossogobius giuris (fish), Panulirus polyphagus (crustacean), and Caulerpa taxifolia (seaweed) exhibited the highest MP contamination within their respective groups. Estimated daily intake (EDI) calculations indicated that children are potentially exposed to approximately three times more MPs than adults through consumption of contaminated seafood (children: 18.11 ± 3.19, 13.65 ± 3.17, and 2.84 ± 0.87 MPs/day for fish, crustaceans, and seaweeds, respectively; adults: 4.49 ± 0.87, 3.72 ± 0.86, and 0.78 ± 0.23 MPs/day, respectively). These findings underscore the urgent need for targeted mitigation strategies, offering a scalable framework for addressing MP contamination in coastal ecosystems globally.}
}
@article{CHEN2025128918,
title = {Empowering the urban fringe: Guerrilla gardening and the creation of informal green spaces},
