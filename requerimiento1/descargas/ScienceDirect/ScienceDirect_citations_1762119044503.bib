@article{PENG2026122816,
title = {M2Emo: Fine-grained mamba transformer guided multi-task golden snub-nosed monkey emotion recognition},
journal = {Information Sciences},
volume = {728},
pages = {122816},
year = {2026},
issn = {0020-0255},
doi = {https://doi.org/10.1016/j.ins.2025.122816},
url = {https://www.sciencedirect.com/science/article/pii/S0020025525009521},
author = {Xianlin Peng and Fangqing Fan and Lingjie Kong and Qiyao Hu and Gu Fang},
keywords = {Multi-task emotion recognition, Multi-scale mamba, Golden snub-nosed monkey},
abstract = {The Qinling golden snub-nosed monkey is a rare and endangered species endemic to China. Its rich facial expressions and complex social behaviors offer a valuable window into ecological research. Recently, various deep learning methods have been applied to individual identification and behavior analysis in primates, enabling preliminary automated monitoring. However, existing approaches face limitations in fine-grained feature modeling, exploiting correlations among tasks, and expressive label mapping. They struggle to effectively integrate multi-dimensional information such as identity, emotion, and gender. To address these challenges, we propose M2Emo, a multi-task recognition framework based on fine-grained Mamba and a lightweight Kolmogorov-Arnold network, for joint individual, emotion, and gender recognition in golden snub-nosed monkeys. We introduce a LocMamba module based on fine-grained Mamba to enhance the perception of local facial details, such as expressions and fur texture. We also propose a GloMamba module that strengthens global semantic representation through sequence modeling of a CLS token. Furthermore, we design a FastKAN multi-task classification head based on the Kolmogorov-Arnold network. It uses learnable spline basis functions to achieve efficient and decoupled mapping for multi-task labels. Importantly, we construct EmoKey, a multi-modal emotion dataset for wild primates. Experimental results show that our method achieves strong performance and good generalization in individual recognition, emotion classification, and gender identification. The code and dataset are available at https://github.com/LPDLG/M2Emo.}
}
