publisher = {Elsevier},
year = {2025},
issn = {0065-2458},
doi = {https://doi.org/10.1016/bs.adcom.2025.06.008},
url = {https://www.sciencedirect.com/science/article/pii/S0065245825000993},
author = {P. Kanmani and K.Chandra Prabha and J. Veerandeswari and M. Manjula},
keywords = {Generative AI, Software engineering automation, AI-driven code generation, Developer productivity, Autonomous software agents},
abstract = {Software engineering is transforming into a new form in this era. With the help of Generative Artificial Intelligence (GenAI), the software engineering phases such as code generation, refactoring, testing, documentation etc., are being automated. Utilizing models such as GitHub Copilot, Amazon CodeWhisperer, and StarCoder, GenAI solutions augment developer productivity by providing real-time code recommendations, debugging support, and natural language-based “vibe coding.” These tools ease the way software production and decrease the time for development; however, they pose new challenges too. Certain challenges are coding “hallucinations,” security flaws, and the spread of bias. It is important to address the challenges listed previously. This requires continuous and strict validation. Along with that there is the need to have explanations for the automation. This could help the new coders to understand and get along with the AI-generated coding. The advent of full-stack autonomous agents, such as Claude from Anthropic, offers the capability to manage end-to-end development with limited human involvement. As GenAI interfaces with IDEs, CI/CD pipelines, and APIs, it cultivates a collaborative software development environment where human creativity and AI enhancement converge, enhancing efficiency and innovation throughout the software lifecycle.}
}
@article{BARA2025113826,
title = {Algorithmic Aesthetics: Cognitive Perspectives on AI-Generated Visual Art},
journal = {iScience},
pages = {113826},
year = {2025},
issn = {2589-0042},
doi = {https://doi.org/10.1016/j.isci.2025.113826},
url = {https://www.sciencedirect.com/science/article/pii/S2589004225020875},
author = {Ionela Bara and Kohinoor M. Darda and Richard Ramsey and Emily S. Cross},
abstract = {Summary
In recent years, the use of generative artificial intelligence (GAI) has proliferated across various domains, ranging from advertising and social media to the generation of visual artwork for presentation at esteemed art exhibitions and for sale on the art auction market. Despite its growing prevalence, empirical data show that people maintain a clear bias against AI-generated visual art when they are aware of its artificial origins. Here we explore why this bias might exist, and whether its existence dooms the eventual acceptance and even celebration of art generated by artificial means. We do this by bringing together recent empirical evidence from the disciplines of neuroaesthetics, social cognition, art history, and human—machine interaction to develop several new perspectives that are ripe for empirical evaluation. Given the growing momentum and outlook of AI-generated art, particularly image generation, in today’s society, our aim with this paper is to provide novel evidence-based propositions for understanding the relationship between visual art and artificial intelligence with a clear focus on social cognitive neuroscience perspectives.}
}
@article{JUNYU2025652,
