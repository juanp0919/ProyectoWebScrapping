@article{BYRGE2025100105,
title = {Artificial Intelligence and the creative process: Does AI-creativity extend beyond divergent thinking?},
journal = {Journal of Creativity},
volume = {35},
number = {2},
pages = {100105},
year = {2025},
issn = {2713-3745},
